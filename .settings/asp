    &lt;!--函数--&gt;<BR>
    &lt;script type=&quot;text/javascript&quot;&gt;<BR>
	$(document).ready(function(){<BR>
		$(&quot;#loginbtn&quot;).click(function(){<BR>
			if ($(&quot;#username&quot;).val()==&quot;&quot; ){<BR>
				alert(&quot;&quot;+&quot;帐号不能为空！&quot;+&quot;&quot;);<BR>
				return;<BR>
			}<BR>
			if ($(&quot;#password&quot;).val()==&quot;&quot; ){<BR>
				alert(&quot;&quot;+&quot;密码不能为空！&quot;+&quot;&quot;);<BR>
				return;<BR>
			}<BR>
			if ($(&quot;#getcode&quot;).val()==&quot;&quot; ){<BR>
				alert(&quot;&quot;+&quot;验证码不能为空！&quot;+&quot;&quot;);<BR>
				return;<BR>
			}<BR>
			var username	= escape($(&quot;#username&quot;).val());<BR>
			var password	= escape($(&quot;#password&quot;).val());<BR>
			var getcode		= escape($(&quot;#getcode&quot;).val());<BR>
			var it			= escape($(&quot;#it&quot;).val());<BR>
			document.getElementById(&quot;loginbtn_status&quot;).innerHTML='&lt;img src=&quot;/public/js/loading.gif&quot; width=&quot;30&quot; /&gt;';<BR>
			//$(&quot;#button1&quot;).hide();<BR>
			//$(&quot;#loginbtn&quot;).toggle();		//提交时，隐藏提交按钮；<BR>
			$.ajax({url:&quot;master_login.Post.asp?Action=Login&quot;,type:&quot;POST&quot;,data:&quot;&quot;<BR>
				+ &quot;Chris=&quot;	//操作.<BR>
				+ &quot;&amp;username=&quot;+ username<BR>
				+ &quot;&amp;password=&quot;+ password<BR>
				+ &quot;&amp;getcode=&quot;+ getcode<BR>
				+ &quot;&amp;it=&quot;+ it<BR>
				+ &quot;&quot;,success: function(msg){<BR>
				if (msg==&quot;success&quot;){<BR>
				//$(&quot;#aaa&quot;).text(&quot;数据处理中。。。&quot;);<BR>
					alert(&quot;&quot; + &quot;登录成功&quot; + &quot;&quot;);<BR>
					document.getElementById(&quot;loginbtn_status&quot;).innerHTML='';<BR>
					//$(&quot;#addbtn&quot;).toggle();	//提交成功时，再显示提交按钮；<BR>
					parent.tb_remove();			//thickbox关闭窗口<BR>
					parent.window.location.reload();	//刷新页面<BR>
				}else{<BR>
					alert( &quot;报警：&quot; + msg + &quot;&quot; );<BR>
					document.getElementById(&quot;loginbtn_status&quot;).innerHTML='';<BR>
				  	//document.write(msg)<BR>
					//parent.tb_remove();			//thickbox关闭窗口<BR>
					parent.window.location.reload();<BR>
				}},error: function(){alert( &quot;&quot; + &quot;页面错误&quot; + &quot;&quot; ); document.getElementById(&quot;loginbtn_status&quot;).innerHTML='';parent.window.location.reload();}});<BR>
		});<BR>
	 });<BR>
	&lt;/script&gt;<BR>
	<BR>
&lt;/body&gt;<BR>
&lt;/html&gt;
</DD>
<DT>master_login.Post.asp</DT>
<!--property:icon_internal=blank-->
<!--property:syntax_highlight=vbnet-->
<!--property:date_created=1364549833-->
<!--property:date_modified=1386920311-->
<DD>
&lt;!--#include file=&quot;../inc/conn.asp&quot; --&gt;<BR>
&lt;!--#include file=&quot;../system/ci_class.asp&quot;--&gt;<BR>
<BR>
&lt;%<BR>
'变量定义区.<BR>
'(用来存储对象的变量，用全大写!)<BR>
Dim TitleName,UnitName<BR>
TitleName 	= &quot;xxx管理&quot;				'此模块管理页的名字.<BR>
UnitName 	= &quot;xxx&quot;					'此模块涉及记录的元素名.<BR>
'自定义设置.<BR>
'本地设置.<BR>
Dim CurrentTableName<BR>
CurrentTableName = &quot;test_wang&quot;		'此模块涉及的[表]名.<BR>
%&gt;<BR>
&lt;%<BR>
Dim RS, sql				'查询列表记录用的变量.<BR>
Dim FoundErr,ErrMsg		'控制错误流程用的控制变量.<BR>
Dim Action				'构建查询字符串以及流程控制用的控制变量.<BR>
Action = CokeShow.filtPublicRequest(Request.QueryString(&quot;Action&quot;))<BR>
<BR>
If Action=&quot;Login&quot; Then<BR>
	Call chkLogin()<BR>
Else<BR>
	'Call Main()<BR>
End If<BR>
<BR>
'拦截.<BR>
If FoundErr=True Then<BR>
	Response.Write ErrMsg<BR>
	Response.End()<BR>
End If<BR>
<BR>
<BR>
Sub chkLogin()<BR>
	Dim newID,newNAME<BR>
	Dim oneFormItem<BR>
	'获取Form表单参数：<BR>
	For Each oneFormItem In Request.Form<BR>
		Execute(&quot;Dim &quot;&amp; oneFormItem)<BR>
		Execute(&quot;&quot;&amp; oneFormItem &amp;&quot; = CokeShow.filtPublicRequest(Request.Form(&quot;&quot;&quot;&amp; oneFormItem &amp;&quot;&quot;&quot;))&quot;)<BR>
	Next<BR>
	'测试：<BR>
		'response.Write username &amp;&quot;&lt;br /&gt;&quot;<BR>
		'response.Write password &amp;&quot;&lt;br /&gt;&quot;<BR>
		'response.Write getcode &amp;&quot;&lt;br /&gt;&quot;<BR>
		'response.Write it &amp;&quot;&lt;br /&gt;&quot;<BR>
		'Response.End()<BR>
	<BR>
	<BR>
	<BR>
	'令牌机制（接收处理）:<BR>
	'Chris Begin.<BR>
	'2013-10-25<BR>
	'技术考虑：先发放一个临时会话变量，凭此才可以有资格、有前提去进行登录PostData！<BR>
	'令牌机制<BR>
	'当前动作：验证【令牌】真伪......<BR>
	'如果验证【令牌】不正确：(要刷新一下页面获得最新令牌！)<BR>
	'it 值不匹配！、 或令牌 StartTime 现在时候已经过期时（表单处自定义了过期时间@），都 统一报警过期！<BR>
	If CokeShow.CheckToken(&quot;it&quot;)=False Then<BR>
		Response.Write &quot;此登录页已过期，请再次进行登录！&quot;<BR>
		Response.End()<BR>
		Exit Sub<BR>
	End If<BR>
	'Chris End.<BR>
	'配套隐藏字段：<BR>
	'&lt;input name=&quot;it&quot; type=&quot;hidden&quot; value=&quot;&lt;% =Replace(CokeShow.PassEncode( Ucase(Md5( Session(&quot;YouCanLoginID_Temp&quot;) )) ),&quot;%&quot;,&quot;&quot;) % &gt;&quot;&gt;<BR>
	<BR>
	'-------------------------------------------------------------------------------<BR>
	'克里斯反导弹系统.<BR>
	'-------------------------------------------------------------------------------<BR>
	'令牌防御（接收处理）:增强令牌防御<BR>
	'Chris Begin<BR>
	'2013-04-26<BR>
	'技术考虑：配合上边令牌机制的增强令牌防御&#8212;&#8212;&#8212;&#8212;&#8212;&#8212;通过令牌，来总理出 IP 访问次数！【接近无敌Chris】<BR>
	'令牌防御系统<BR>
	'当前动作：Application 对象计数器，通过不同的 IP，分别记下统计对应 IP 的 &quot;登录次数&quot; ~<BR>
	'如果启动【令牌防御】不成功：(要跳转、提示并截停等处理！)<BR>
	'面向对象编程实现：极度简洁：<BR>
	If CokeShow.TokenSafeGuard(&quot;占用参数.后一参数表示可容忍e'可能攻击次数'...&quot;, 20)=False Then<BR>
		Response.Write &quot;启动防御系统失效！Impossible&quot;<BR>
		Response.End()<BR>
		Exit Sub<BR>
	End If<BR>
	'Chris End.<BR>
	'-------------------------------------------------------------------------------<BR>
	'克里斯反导弹系统.<BR>
	'-------------------------------------------------------------------------------<BR>
	<BR>
	<BR>
	<BR>
	'验证：<BR>
	If username=&quot;&quot; Then<BR>
		FoundErr = True<BR>
		ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;帐号&quot; &amp;&quot;不能为空！&quot;<BR>
	Else<BR>
		If CokeShow.strLength(username)&gt;30 Or CokeShow.strLength(username)&lt;6 Then<BR>
			FoundErr = True<BR>
			ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;帐号&quot; &amp;&quot;长度不能大于30个字符，也不能小于6个字符！&quot;<BR>
		Else<BR>
			username = username<BR>
		End If<BR>
	End If<BR>
	<BR>
	If password=&quot;&quot; Then<BR>
		FoundErr = True<BR>
		ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;密码&quot; &amp;&quot;不能为空！&quot;<BR>
	Else<BR>
		If CokeShow.strLength(password)&gt;30 Or CokeShow.strLength(password)&lt;6 Then<BR>
			FoundErr = True<BR>
			ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;密码&quot; &amp;&quot;长度不能大于30个字符，也不能小于6个字符！&quot;<BR>
		Else<BR>
			password = password<BR>
		End If<BR>
	End If<BR>
	<BR>
	If getcode=&quot;&quot; Then<BR>
		FoundErr = True<BR>
		ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;验证码&quot; &amp;&quot;不能为空！&quot;<BR>
	Else<BR>
		If isNumeric(getcode) Then<BR>
			'getcode = CokeShow.CokeClng(getcode)<BR>
			If CStr(getcode)&lt;&gt;CStr(Session(&quot;GetCode&quot;)) Then<BR>
				FoundErr = True<BR>
				ErrMsg = ErrMsg &amp;&quot;验证码&quot;&amp; &quot;&quot; &amp;&quot;不正确！&quot;<BR>
			End If<BR>
		Else<BR>
			FoundErr = True<BR>
			ErrMsg = ErrMsg &amp;&quot;验证码&quot;&amp; &quot;&quot; &amp;&quot;请用数字填写！&quot;<BR>
		End If<BR>
	End If<BR>
	<BR>
	'拦截错误，不然错误往下进行！<BR>
	If FoundErr=True Then Exit Sub<BR>
	<BR>
	<BR>
	<BR>
	Set RS = Server.CreateObject(&quot;Adodb.RecordSet&quot;)<BR>
	sql = &quot;&quot;&amp;_<BR>
			&quot;SELECT TOP 1 &quot;&amp;_<BR>
				&quot;user_userID,user_userUsername,user_userPassword,user_groupID &quot;&amp;_<BR>
				&quot;,user_userName,user_userSex &quot;&amp;_<BR>
				&quot;,user_userExpiredTime,user_userLastloginTime,user_userLastloginIP &quot;&amp;_<BR>
				&quot;,user_groupName,user_groupPower &quot;&amp;_<BR>
			&quot;FROM [user_user] u &quot;&amp;_<BR>
				&quot;LEFT JOIN [user_group] g ON( u.user_groupID = g.user_groupID AND g.user_groupIsDeleted=0 ) &quot;&amp;_<BR>
			&quot;WHERE &quot;&amp;_<BR>
				&quot;0=0 AND user_userIsDeleted=0 &quot;&amp;_<BR>
				&quot;AND user_username='&quot; &amp; username &amp; &quot;' &quot;&amp;_<BR>
				&quot;AND user_password='&quot; &amp; md5(password) &amp; &quot;' &quot;&amp;_<BR>
			&quot;ORDER BY user_userID&quot;<BR>
	'Call CokeShow.AddLog(999, &quot;管理员登录&quot;, 9999, &quot;删除操作：成功清除了广告ID为&quot;&amp; SelectID &amp;&quot;的所有图库和字库！并删除了广告&quot;&amp; SelectID &amp;&quot; &quot;)<BR>
	'RS.Open sql,CONN,1,1<BR>
	RS.Open sql,ConnStr,adOpenKeyset,adLockReadOnly,adCmdText<BR>
	response.Write &quot;333&quot;<BR>
	If RS.Eof Then <BR>
		Call CokeShow.AddLog(0, &quot;管理员登录&quot;, 0, &quot;登陆操作：【失败】尝试管理员 [&quot;&amp; username &amp;&quot;,&quot;&amp; password &amp;&quot;] 于 [&quot;&amp; Now() &amp;&quot;] 在 [&quot;&amp; CokeShow.getIPAddress() &amp;&quot;] [&quot;&amp; get_ip_CityAddress( CokeShow.getIPAddress() ) &amp;&quot;] 登陆失败&quot;)<BR>
		Response.Write &quot;用户名或密码错误，请返回重试&quot;<BR>
		Response.End()<BR>
	Else<BR>
		If DateDiff(&quot;d&quot;, RS(&quot;user_userExpiredTime&quot;), Now())&gt;=0 Then<BR>
			Response.Write &quot;你的密码已经过期！无法登录系统，请与管理员联系。&quot;<BR>
			Call CokeShow.AddLog(0, &quot;管理员登录&quot;, 0, &quot;登陆操作：【异常】管理员 [&quot;&amp; username &amp;&quot;] 于 [&quot;&amp; Now() &amp;&quot;] 在 [&quot;&amp; CokeShow.getIPAddress() &amp;&quot;] [&quot;&amp; get_ip_CityAddress( CokeShow.getIPAddress() ) &amp;&quot;] 登陆被拒绝&quot;)<BR>
			Response.End()<BR>
		End If<BR>
		<BR>
		Session(&quot;user_userID&quot;)				= RS(&quot;user_userID&quot;)<BR>
		Session(&quot;user_userUsername&quot;)		= RS(&quot;user_userUsername&quot;)<BR>
		Session(&quot;user_userName&quot;)			= RS(&quot;user_userName&quot;)<BR>
		Session(&quot;user_userExpiredTime&quot;)		= RS(&quot;user_userExpiredTime&quot;)<BR>
		Session(&quot;user_userSex&quot;)				= RS(&quot;user_userSex&quot;)<BR>
		Session(&quot;user_userLastloginTime&quot;)	= RS(&quot;user_userLastloginTime&quot;)<BR>
		Session(&quot;user_userLastloginIP&quot;)		= RS(&quot;user_userLastloginIP&quot;)<BR>
		Session(&quot;user_groupName&quot;)			= RS(&quot;user_groupName&quot;)<BR>
		Session(&quot;user_groupPower&quot;)			= RS(&quot;user_groupPower&quot;)<BR>
<BR>
		Dim cookiesKey<BR>
		Randomize<BR>
		cookiesKey = md5(Rnd())<BR>
		Conn.Execute(&quot;UPDATE [user_user] SET user_userLastloginTime=GETDATE(),user_userLastloginIP='&quot;&amp; CokeShow.getIPAddress() &amp;&quot;',user_userCookiesKey='&quot;&amp; cookiesKey &amp;&quot;' WHERE 0=0 AND user_userIsDeleted=0 AND user_userID=&quot;&amp; RS(&quot;user_userID&quot;))<BR>
		Response.Cookies(&quot;cookies_admin&quot;)(&quot;Admin_ID&quot;)		= RS(&quot;user_userID&quot;)<BR>
		Response.Cookies(&quot;cookies_admin&quot;)(&quot;Admin_Power&quot;)	= RS(&quot;user_groupPower&quot;)<BR>
		Response.Cookies(&quot;cookies_admin&quot;)(&quot;Admin_PublicKey&quot;)= md5(RS(&quot;user_userID&quot;) &amp;&quot;zLheCLsa358E&quot;&amp; RS(&quot;user_groupPower&quot;))<BR>
		Response.Cookies(&quot;cookies_admin&quot;)(&quot;Admin_PrivateKey&quot;)= md5(RS(&quot;user_userID&quot;) &amp; cookiesKey &amp; RS(&quot;user_groupPower&quot;))<BR>
		<BR>
		Call CokeShow.AddLog(0, &quot;管理员登录&quot;, 0, &quot;登陆操作：【成功】管理员 [&quot;&amp; username &amp;&quot;] 于 [&quot;&amp; Now() &amp;&quot;] 在 [&quot;&amp; CokeShow.getIPAddress() &amp;&quot;] [&quot;&amp; get_ip_CityAddress( CokeShow.getIPAddress() ) &amp;&quot;] 登陆成功&quot;)<BR>
		<BR>
		<BR>
		<BR>
		'-------------------------------------------------------------------------------<BR>
		'克里斯反导弹系统.<BR>
		'-------------------------------------------------------------------------------<BR>
		'Chris<BR>
		'Unlock解锁：IP-攻击次数限制：<BR>
		If CokeShow.TokenSafeGuard_Unlock( CokeShow.getIPAddress() )=True Then Response.Write &quot;&quot;<BR>
		'Chris<BR>
		'-------------------------------------------------------------------------------<BR>
		'克里斯反导弹系统.<BR>
		'-------------------------------------------------------------------------------<BR>
		<BR>
	End If<BR>
	RS.Close<BR>
	Set RS = Nothing<BR>
	<BR>
	Response.Write &quot;success&quot;<BR>
End Sub<BR>
<BR>

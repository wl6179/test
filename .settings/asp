	<BR>
	'检查验证码是否正确.<BR>
    Public Function CodePass()<BR>
        Dim CodeStr<BR>
        CodeStr = CokeShow.filtRequest(Request(&quot;CodeStr&quot;))					'用户填写的验证码的字符串.<BR>
        If CStr(Session(&quot;GetCode&quot;))=CStr(CodeStr) And CodeStr&lt;&gt;&quot;&quot; Then		'对比产生的验证码是否与用户填写的一致.<BR>
            CodePass = True<BR>
            Session(&quot;GetCode&quot;)=Empty	'立刻销毁产生的验证码.<BR>
        Else<BR>
            CodePass = False<BR>
            Session(&quot;GetCode&quot;)=Empty	'立刻销毁产生的验证码.<BR>
        End If<BR>
    End Function<BR>
	'检查验证码是否正确.自定义.专门针对Ajax远程表单的验证码匹配函数.<BR>
	'strCodeStrName：验证码的input的name属性值.<BR>
    Public Function CodePassII(strCodeStrName)<BR>
        Dim CodeStr<BR>
        CodeStr = CokeShow.filtRequest(Request(strCodeStrName))					'用户填写的验证码的字符串.<BR>
        If CStr(Session(&quot;GetCode&quot;))=CStr(CodeStr) And CodeStr&lt;&gt;&quot;&quot; Then		'对比产生的验证码是否与用户填写的一致.<BR>
            CodePassII = True<BR>
            Session(&quot;GetCode&quot;)=Empty	'立刻销毁产生的验证码.<BR>
        Else<BR>
            CodePassII = False<BR>
            '//不立刻销毁验证码Session(&quot;GetCode&quot;)=Empty	'立刻销毁产生的验证码.<BR>
        End If<BR>
    End Function<BR>
	<BR>
	<BR>
	'有问题！！！！！！！！！（其实没问题，是有一些 水军使用 本地（中文名）.htm 页面来提交留言时，中文名称没有经过URL处理而导致的！出错！【这个函数必须经过URL编码中文后，才能使用！】）<BR>
	Function URLDecode(enStr)<BR>
		'Chris.<BR>
		If Instr(enStr,&quot;file&quot;)&gt;0 And Instr(enStr,&quot;Documents&quot;)&gt;0 Then<BR>
			enStr = Server.URLEncode(enStr)<BR>
		Else<BR>
			enStr = enStr<BR>
		End If<BR>
		<BR>
		Dim deStr,strSpecial<BR>
		Dim c,i,v<BR>
		deStr=&quot;&quot;<BR>
		strSpecial=&quot;!&quot;&quot;#$%&amp;'()*+,.-_/:;&lt;=&gt;?@[\]^`{|}~%&quot;<BR>
		For i=1 To Len(enStr)<BR>
			c = Mid(enStr, i, 1)<BR>
			If c=&quot;%&quot; Then<BR>
				v = Eval(&quot;&amp;h&quot; + Mid(enStr, i+1,2))<BR>
				If inStr(strSpecial, Chr(v))&gt;0 Then<BR>
					deStr = deStr &amp; Chr(v)<BR>
					i = i+2<BR>
				Else<BR>
					v = Eval(&quot;&amp;h&quot; + Mid(enStr, i+1,2) + Mid(enStr, i+4,2))<BR>
					deStr = deStr &amp; Chr(v)<BR>
					i = i + 5<BR>
				End If<BR>
			Else<BR>
				If c=&quot;+&quot; Then<BR>
					deStr = deStr &amp; &quot; &quot;<BR>
				Else<BR>
					deStr = deStr &amp; c<BR>
				End If<BR>
			End If<BR>
		Next<BR>
		URLDecode=deStr<BR>
	End Function<BR>
	<BR>
	<BR>
	<BR>
	<BR>
	<BR>
	'转换成 仅中英文 的安全字符（仅针对URL的Encode字符进行编码）.<BR>
	'strCodeStrName：需要处理的URL的Encode字符.<BR>
    Public Function MyChangeForURLEncode(strCodeStrName)<BR>
        MyChangeForURLEncode = &quot;&quot;<BR>
		<BR>
		MyChangeForURLEncode = Replace( strCodeStrName, &quot;-&quot;, &quot;AK43&quot; )<BR>
		MyChangeForURLEncode = Replace( MyChangeForURLEncode, &quot;_&quot;, &quot;AK44&quot; )<BR>
		MyChangeForURLEncode = Replace( MyChangeForURLEncode, &quot;%&quot;, &quot;AK45&quot; ):MyChangeForURLEncode = Replace( MyChangeForURLEncode, &quot;％&quot;, &quot;AK45&quot; )	'特殊处理 chkcode 安全函数遗留问题.<BR>
		MyChangeForURLEncode = Replace( MyChangeForURLEncode, &quot;//&quot;, &quot;AK46&quot; )	'双斜杠 必须摆在 前面！<BR>
		MyChangeForURLEncode = Replace( MyChangeForURLEncode, &quot;/&quot;, &quot;AK47&quot; )<BR>
		<BR>
		MyChangeForURLEncode = MyChangeForURLEncode<BR>
    End Function<BR>
	<BR>
	'将 仅中英文 的安全字符 进行解码！（仅针对 已编码的 私人安全字符 进行解码）.<BR>
	'strCodeStrName：需要处理的URL的Encode字符.<BR>
    Public Function UnMyChangeForURLEncode(strCodeStrName)<BR>
        UnMyChangeForURLEncode = &quot;&quot;<BR>
		<BR>
		UnMyChangeForURLEncode = Replace( strCodeStrName, &quot;AK43&quot;, &quot;-&quot; )<BR>
		UnMyChangeForURLEncode = Replace( UnMyChangeForURLEncode, &quot;AK44&quot;, &quot;_&quot; )<BR>
		UnMyChangeForURLEncode = Replace( UnMyChangeForURLEncode, &quot;AK45&quot;, &quot;%&quot; )<BR>
		UnMyChangeForURLEncode = Replace( UnMyChangeForURLEncode, &quot;AK46&quot;, &quot;//&quot; )	'双斜杠<BR>
		UnMyChangeForURLEncode = Replace( UnMyChangeForURLEncode, &quot;AK47&quot;, &quot;/&quot; )<BR>
		<BR>
		UnMyChangeForURLEncode = UnMyChangeForURLEncode<BR>
    End Function<BR>
	<BR>
	<BR>
	<BR>
	'将数字 进行编码（仅针对电话号码）.<BR>
	'strCodeStrName：需要处理的数字字符.<BR>
    Public Function TelChangeEncode(strCodeStrName)<BR>
        TelChangeEncode = &quot;&quot;<BR>
		<BR>
		TelChangeEncode = Replace( strCodeStrName,  &quot;1&quot;, &quot;{AK-a}&quot; )<BR>
		TelChangeEncode = Replace( TelChangeEncode, &quot;3&quot;, &quot;{AK-b}&quot; )<BR>
		TelChangeEncode = Replace( TelChangeEncode, &quot;5&quot;, &quot;{AK-c}&quot; )<BR>
		TelChangeEncode = Replace( TelChangeEncode, &quot;7&quot;, &quot;{AK-d}&quot; )<BR>
		TelChangeEncode = Replace( TelChangeEncode, &quot;9&quot;, &quot;{AK-e}&quot; )<BR>
		TelChangeEncode = Replace( TelChangeEncode, &quot;0&quot;, &quot;{AK-f}&quot; )<BR>
		TelChangeEncode = Replace( TelChangeEncode, &quot;2&quot;, &quot;{AK-g}&quot; )<BR>
		TelChangeEncode = Replace( TelChangeEncode, &quot;4&quot;, &quot;{AK-h}&quot; )<BR>
		TelChangeEncode = Replace( TelChangeEncode, &quot;6&quot;, &quot;{AK-i}&quot; )<BR>
		TelChangeEncode = Replace( TelChangeEncode, &quot;8&quot;, &quot;{AK-j}&quot; )<BR>
				<BR>
		TelChangeEncode = TelChangeEncode<BR>
    End Function<BR>
	<BR>
	'将 仅中英文 的安全字符 进行解码！（成数字）<BR>
	'strCodeStrName：需要处理的安全字符.<BR>
    Public Function UnTelChangeEncode(strCodeStrName)<BR>
        UnTelChangeEncode = &quot;&quot;<BR>
		<BR>
		UnTelChangeEncode = Replace( strCodeStrName,    &quot;{AK-j}&quot;, &quot;8&quot; )<BR>
		UnTelChangeEncode = Replace( UnTelChangeEncode, &quot;{AK-i}&quot;, &quot;6&quot; )<BR>
		UnTelChangeEncode = Replace( UnTelChangeEncode, &quot;{AK-h}&quot;, &quot;4&quot; )<BR>
		UnTelChangeEncode = Replace( UnTelChangeEncode, &quot;{AK-g}&quot;, &quot;2&quot; )<BR>
		UnTelChangeEncode = Replace( UnTelChangeEncode, &quot;{AK-f}&quot;, &quot;0&quot; )<BR>
		UnTelChangeEncode = Replace( UnTelChangeEncode, &quot;{AK-e}&quot;, &quot;9&quot; )<BR>
		UnTelChangeEncode = Replace( UnTelChangeEncode, &quot;{AK-d}&quot;, &quot;7&quot; )<BR>
		UnTelChangeEncode = Replace( UnTelChangeEncode, &quot;{AK-c}&quot;, &quot;5&quot; )<BR>
		UnTelChangeEncode = Replace( UnTelChangeEncode, &quot;{AK-b}&quot;, &quot;3&quot; )<BR>
		UnTelChangeEncode = Replace( UnTelChangeEncode, &quot;{AK-a}&quot;, &quot;1&quot; )<BR>
		<BR>
		UnTelChangeEncode = UnTelChangeEncode<BR>
    End Function<BR>

<DD>
&lt;!--#include file=&quot;../inc/conn.asp&quot; --&gt;<BR>
&lt;%<BR>
'变量定义区.<BR>
'(用来存储对象的变量，用全大写!)<BR>
Dim TitleName,UnitName<BR>
TitleName 	= &quot;管理人员管理&quot;				'此模块管理页的名字.<BR>
UnitName 	= &quot;管理人员&quot;					'此模块涉及记录的元素名.<BR>
'自定义设置.<BR>
'本地设置.<BR>
Dim CurrentTableName<BR>
CurrentTableName = &quot;[user_user]&quot;			'此模块涉及的[表]名.<BR>
%&gt;<BR>
&lt;%<BR>
Dim RS, sql				'查询列表记录用的变量.<BR>
Dim FoundErr,ErrMsg		'控制错误流程用的控制变量.<BR>
Dim Action				'构建查询字符串以及流程控制用的控制变量.<BR>
Action = CokeShow.filtPublicRequest(Request.QueryString(&quot;Action&quot;))<BR>
<BR>
If Action=&quot;SaveAdd&quot; Then<BR>
	Call SaveAdd()<BR>
ElseIf Action=&quot;SaveModify&quot; Then<BR>
	Call SaveModify()<BR>
ElseIf Action=&quot;Delete&quot; Then<BR>
	Call Delete()<BR>
ElseIf Action=&quot;Lock&quot; Then<BR>
	Call Lock()<BR>
ElseIf Action=&quot;UnLock&quot; Then<BR>
	Call UnLock()<BR>
Else<BR>
	'Call Main()<BR>
End If<BR>
<BR>
'拦截.<BR>
If FoundErr=True Then<BR>
	Response.Write ErrMsg<BR>
	Response.End()<BR>
End If<BR>
<BR>
Sub SaveAdd()<BR>
	Dim newID,newNAME<BR>
	Dim oneFormItem<BR>
	'获取Form表单参数：<BR>
	For Each oneFormItem In Request.Form<BR>
		Execute(&quot;Dim &quot;&amp; oneFormItem)<BR>
		Execute(&quot;&quot;&amp; oneFormItem &amp;&quot; = CokeShow.filtPublicRequest(Request.Form(&quot;&quot;&quot;&amp; oneFormItem &amp;&quot;&quot;&quot;))&quot;)<BR>
	Next<BR>
	'测试：<BR>
		'response.Write user_userUsername &amp;&quot;&lt;br /&gt;&quot;<BR>
		'response.Write user_userPassword &amp;&quot;&lt;br /&gt;&quot;<BR>
		'response.Write user_userPassword_confirm &amp;&quot;&lt;br /&gt;&quot;<BR>
		'response.Write user_groupID &amp;&quot;&lt;br /&gt;&quot;<BR>
		'response.Write user_userName &amp;&quot;&lt;br /&gt;&quot;<BR>
		'response.Write user_userSex &amp;&quot;&lt;br /&gt;&quot;<BR>
		'response.Write user_userTel &amp;&quot;&lt;br /&gt;&quot;<BR>
		'response.Write user_userEmail &amp;&quot;&lt;br /&gt;&quot;<BR>
		'response.Write user_userQQ &amp;&quot;&lt;br /&gt;&quot;<BR>
		'response.Write user_userRemark &amp;&quot;&lt;br /&gt;&quot;<BR>
		'response.Write Action &amp;&quot;&lt;br /&gt;&quot;<BR>
		'Response.End()<BR>
	<BR>
	'验证：<BR>
	'字符类:<BR>
	If user_userUsername=&quot;&quot; Then<BR>
		FoundErr = True<BR>
		ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;帐号&quot; &amp;&quot;不能为空！&quot;<BR>
	Else<BR>
		If CokeShow.strLength(user_userUsername)&gt;30 Or CokeShow.strLength(user_userUsername)&lt;6 Then<BR>
			FoundErr = True<BR>
			ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;帐号&quot; &amp;&quot;长度不能大于30个字符，也不能小于6个字符！&quot;<BR>
		Else<BR>
			user_userUsername = user_userUsername<BR>
		End If<BR>
	End If<BR>
	<BR>
	If user_userPassword=&quot;&quot; Then<BR>
		FoundErr = True<BR>
		ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;密码&quot; &amp;&quot;不能为空！&quot;<BR>
	Else<BR>
		If CokeShow.strLength(user_userPassword)&gt;30 Or CokeShow.strLength(user_userPassword)&lt;6 Then<BR>
			FoundErr = True<BR>
			ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;密码&quot; &amp;&quot;长度不能大于30个字符，也不能小于6个字符！&quot;<BR>
		Else<BR>
			user_userPassword = user_userPassword<BR>
		End If<BR>
	End If<BR>
	<BR>
	If user_userPassword_confirm&lt;&gt;user_userPassword Then	'都为空，也可以！<BR>
		FoundErr = True<BR>
		ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;确认密码&quot; &amp;&quot;和密码不对等！&quot;<BR>
	End If<BR>
	<BR>
	If user_userName=&quot;&quot; Then<BR>
		'FoundErr = True<BR>
		'ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;姓名&quot; &amp;&quot;不能为空！&quot;<BR>
	Else<BR>
		If CokeShow.strLength(user_userName)&gt;10 Or CokeShow.strLength(user_userName)&lt;1 Then<BR>
			FoundErr = True<BR>
			ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;姓名&quot; &amp;&quot;长度不能大于10个字符，也不能小于1个字符！&quot;<BR>
		Else<BR>
			user_userName = user_userName<BR>
		End If<BR>
	End If<BR>
	<BR>
	If user_userTel=&quot;&quot; Then<BR>
		'FoundErr = True<BR>
		'ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;电话&quot; &amp;&quot;不能为空！&quot;<BR>
	Else<BR>
		If CokeShow.strLength(user_userTel)&gt;50 Or CokeShow.strLength(user_userTel)&lt;8 Then<BR>
			FoundErr = True<BR>
			ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;电话&quot; &amp;&quot;长度不能大于30个字符，也不能小于6个字符！&quot;<BR>
		Else<BR>
			user_userTel = user_userTel<BR>
		End If<BR>
	End If<BR>
	<BR>
	If user_userEmail=&quot;&quot; Then<BR>
		'FoundErr = True<BR>
		'ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;邮箱&quot; &amp;&quot;不能为空！&quot;<BR>
	Else<BR>
		If CokeShow.strLength(user_userEmail)&gt;50 Or CokeShow.strLength(user_userEmail)&lt;8 Then<BR>
			FoundErr = True<BR>
			ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;邮箱&quot; &amp;&quot;长度不能大于50个字符，也不能小于8个字符！&quot;<BR>
		Else<BR>
			user_userEmail = user_userEmail<BR>
		End If<BR>
	End If<BR>
	<BR>
	If user_userQQ=&quot;&quot; Then<BR>
		'FoundErr = True<BR>
		'ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;QQ&quot; &amp;&quot;不能为空！&quot;<BR>
	Else<BR>
		If CokeShow.strLength(user_userQQ)&gt;12 Or CokeShow.strLength(user_userQQ)&lt;5 Then<BR>
			FoundErr = True<BR>
			ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;QQ&quot; &amp;&quot;长度不能大于12个字符，也不能小于5个字符！&quot;<BR>
		Else<BR>
			user_userQQ = user_userQQ<BR>
		End If<BR>
	End If<BR>
	<BR>
	If user_userRemark=&quot;&quot; Then<BR>
		'FoundErr = True<BR>
		'ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;备注&quot; &amp;&quot;不能为空！&quot;<BR>
	Else<BR>
		If CokeShow.strLength(user_userRemark)&gt;250 Or CokeShow.strLength(user_userRemark)&lt;3 Then<BR>
			FoundErr = True<BR>
			ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;备注&quot; &amp;&quot;长度不能大于250个字符，也不能小于3个字符！&quot;<BR>
		Else<BR>
			user_userRemark = user_userRemark<BR>
		End If<BR>
	End If<BR>
	<BR>
	'数字类:<BR>
	If user_groupID=&quot;&quot; Then<BR>
		'FoundErr = True<BR>
		'ErrMsg = ErrMsg &amp;&quot;&quot;&amp; UnitName &amp;&quot;不能为空！&quot;<BR>
		user_groupID = 0<BR>
	Else<BR>
		If isNumeric(user_groupID) Then<BR>
			user_groupID = CokeShow.CokeClng(user_groupID)<BR>
		Else<BR>
			FoundErr = True<BR>
			ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;所属组&quot; &amp;&quot;参数不对！&quot;<BR>
		End If<BR>
	End If<BR>
	<BR>
	If user_userSex=&quot;&quot; Then<BR>
		'FoundErr = True<BR>
		'ErrMsg = ErrMsg &amp;&quot;&quot;&amp; UnitName &amp;&quot;不能为空！&quot;<BR>
		user_userSex = 0<BR>
	Else<BR>
		If isNumeric(user_userSex) Then<BR>
			user_userSex = CokeShow.CokeClng(user_userSex)<BR>
			If (user_userSex)&gt;2 Then<BR>
				FoundErr = True<BR>
				ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;性别&quot; &amp;&quot;参数不对!！&quot;<BR>
			End If<BR>
			<BR>
		Else<BR>
			FoundErr = True<BR>
			ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;性别&quot; &amp;&quot;参数不对！&quot;<BR>
		End If<BR>
	End If<BR>
	<BR>
	'其它类型验证:<BR>
	<BR>
	<BR>
	'拦截错误，不然错误往下进行！<BR>
	If FoundErr=True Then Exit Sub<BR>
	<BR>
	Set RS = Server.CreateObject(&quot;Adodb.RecordSet&quot;)<BR>
	'中途增加了+对重复帐号的验证：<BR>
	sql = &quot;SELECT * FROM &quot;&amp; CurrentTableName &amp;&quot; WHERE 0=0 AND user_userIsDeleted=0 AND user_userUsername='&quot;&amp; user_userUsername &amp;&quot;'&quot;<BR>
	'RS.Open sql,CONN,2,2<BR>
	RS.Open sql,ConnStr,adOpenDynamic,adLockPessimistic,adCmdText<BR>
	'中途增加了+对重复帐号的验证：<BR>
	If Not RS.Eof Then<BR>
		Response.Write &quot;您的帐号&quot;&amp; user_userUsername &amp;&quot;有重复！！&quot;<BR>
		Response.End()<BR>
	End If<BR>
	RS.AddNew<BR>
		RS(&quot;user_userUsername&quot;)	= user_userUsername<BR>
		RS(&quot;user_userPassword&quot;)	= md5(user_userPassword)			'添加时，一定必填新密码！<BR>
		RS(&quot;user_groupID&quot;)		= user_groupID<BR>
		RS(&quot;user_userName&quot;)		= user_userName<BR>
		RS(&quot;user_userSex&quot;)		= user_userSex<BR>
		RS(&quot;user_userTel&quot;)		= user_userTel<BR>
		RS(&quot;user_userEmail&quot;)	= user_userEmail<BR>
		RS(&quot;user_userQQ&quot;)		= user_userQQ<BR>
		RS(&quot;user_userRemark&quot;)	= user_userRemark<BR>
		<BR>
		RS(&quot;user_userExpiredTime&quot;)		= Now()+7					'只要更新帐号，就给予延长7天的有效期期限！<BR>
		RS(&quot;user_userPassword&quot;)			= md5(user_userPassword)	'添加时，一定必填新密码！<BR>
		'RS(&quot;user_userLastloginTime&quot;)	= remark<BR>
		'RS(&quot;user_userLastloginIP&quot;)		= remark<BR>
		'RS(&quot;user_userAddTime&quot;)			= remark<BR>
		'RS(&quot;user_userIsDeleted&quot;)		= remark<BR>
		'RS(&quot;user_userCookiesKey&quot;)		= remark<BR>
	RS.Update<BR>
	'获取新增的ID：<BR>
	RS.MoveLast<BR>
	newID	= RS(&quot;user_userID&quot;)<BR>
	newNAME	= RS(&quot;user_userName&quot;)<BR>
	RS.Close<BR>
	Set RS=Nothing<BR>
	<BR>
	'记入日志.<BR>
	Call CokeShow.AddLog(0, CurrentTableName, newID, &quot;大后台添加操作：成功添加了ID为&quot;&amp; newID &amp;&quot;(&quot;&amp; newNAME &amp;&quot;)的&quot;&amp; UnitName &amp;&quot;！&quot;)<BR>
	Response.Write &quot;success&quot;<BR>
End Sub<BR>
<BR>
Sub SaveModify()<BR>
	Dim intID<BR>
	Dim newID,newNAME<BR>
	Dim oneFormItem<BR>
	'获取Form表单参数：<BR>
	For Each oneFormItem In Request.Form<BR>
		Execute(&quot;Dim &quot;&amp; oneFormItem)<BR>
		Execute(&quot;&quot;&amp; oneFormItem &amp;&quot; = CokeShow.filtPublicRequest(Request.Form(&quot;&quot;&quot;&amp; oneFormItem &amp;&quot;&quot;&quot;))&quot;)<BR>
	Next<BR>
	'测试：<BR>
		'response.Write id &amp;&quot;&lt;br /&gt;&quot;<BR>
		'response.Write user_userUsername &amp;&quot;&lt;br /&gt;&quot;<BR>
		'response.Write user_userPassword &amp;&quot;&lt;br /&gt;&quot;<BR>
		'response.Write user_userPassword_confirm &amp;&quot;&lt;br /&gt;&quot;<BR>
		'response.Write user_groupID &amp;&quot;&lt;br /&gt;&quot;<BR>
		'response.Write user_userName &amp;&quot;&lt;br /&gt;&quot;<BR>
		'response.Write user_userSex &amp;&quot;&lt;br /&gt;&quot;<BR>
		'response.Write user_userTel &amp;&quot;&lt;br /&gt;&quot;<BR>
		'response.Write user_userEmail &amp;&quot;&lt;br /&gt;&quot;<BR>
		'response.Write user_userQQ &amp;&quot;&lt;br /&gt;&quot;<BR>
		'response.Write user_userRemark &amp;&quot;&lt;br /&gt;&quot;<BR>
		'response.Write Action &amp;&quot;&lt;br /&gt;&quot;<BR>
		'Response.End()<BR>
	<BR>
	intID = id		'检测 id 参数.<BR>
	If intID=&quot;&quot; Then<BR>
		FoundErr = True<BR>
		ErrMsg = ErrMsg &amp;&quot;参数不足！&quot;<BR>
		Exit Sub<BR>
	Else<BR>
		intID = CokeShow.CokeClng(intID)<BR>
	End If<BR>
	<BR>
	'验证：<BR>
	'字符类:<BR>
	If user_userUsername=&quot;&quot; Then<BR>
		FoundErr = True<BR>
		ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;帐号&quot; &amp;&quot;不能为空！&quot;<BR>
	Else<BR>
		If CokeShow.strLength(user_userUsername)&gt;30 Or CokeShow.strLength(user_userUsername)&lt;6 Then<BR>
			FoundErr = True<BR>
			ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;帐号&quot; &amp;&quot;长度不能大于30个字符，也不能小于6个字符！&quot;<BR>
		Else<BR>
			user_userUsername = user_userUsername<BR>
		End If<BR>
	End If<BR>
	<BR>
	If user_userPassword=&quot;&quot; Then<BR>
		'FoundErr = True<BR>
		'ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;密码&quot; &amp;&quot;不能为空！&quot;		'可以为空！！！~<BR>
	Else<BR>
		If CokeShow.strLength(user_userPassword)&gt;30 Or CokeShow.strLength(user_userPassword)&lt;6 Then<BR>
			FoundErr = True<BR>
			ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;密码&quot; &amp;&quot;长度不能大于30个字符，也不能小于6个字符！&quot;<BR>
		Else<BR>
			user_userPassword = user_userPassword<BR>
		End If<BR>
	End If<BR>
	<BR>
	If user_userPassword_confirm&lt;&gt;user_userPassword Then	'都为空，也可以！<BR>
		FoundErr = True<BR>
		ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;确认密码&quot; &amp;&quot;和密码不对等！&quot;<BR>
	End If<BR>
	<BR>
	If user_userName=&quot;&quot; Then<BR>
		'FoundErr = True<BR>
		'ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;姓名&quot; &amp;&quot;不能为空！&quot;<BR>
	Else<BR>
		If CokeShow.strLength(user_userName)&gt;10 Or CokeShow.strLength(user_userName)&lt;1 Then<BR>
			FoundErr = True<BR>
			ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;姓名&quot; &amp;&quot;长度不能大于10个字符，也不能小于1个字符！&quot;<BR>
		Else<BR>
			user_userName = user_userName<BR>
		End If<BR>
	End If<BR>
	<BR>
	If user_userTel=&quot;&quot; Then<BR>
		'FoundErr = True<BR>
		'ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;电话&quot; &amp;&quot;不能为空！&quot;<BR>
	Else<BR>
		If CokeShow.strLength(user_userTel)&gt;50 Or CokeShow.strLength(user_userTel)&lt;8 Then<BR>
			FoundErr = True<BR>
			ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;电话&quot; &amp;&quot;长度不能大于30个字符，也不能小于6个字符！&quot;<BR>
		Else<BR>
			user_userTel = user_userTel<BR>
		End If<BR>
	End If<BR>
	<BR>
	If user_userEmail=&quot;&quot; Then<BR>
		'FoundErr = True<BR>
		'ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;邮箱&quot; &amp;&quot;不能为空！&quot;<BR>
	Else<BR>
		If CokeShow.strLength(user_userEmail)&gt;50 Or CokeShow.strLength(user_userEmail)&lt;8 Then<BR>
			FoundErr = True<BR>
			ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;邮箱&quot; &amp;&quot;长度不能大于50个字符，也不能小于8个字符！&quot;<BR>
		Else<BR>
			user_userEmail = user_userEmail<BR>
		End If<BR>
	End If<BR>
	<BR>
	If user_userQQ=&quot;&quot; Then<BR>
		'FoundErr = True<BR>
		'ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;QQ&quot; &amp;&quot;不能为空！&quot;<BR>
	Else<BR>
		If CokeShow.strLength(user_userQQ)&gt;12 Or CokeShow.strLength(user_userQQ)&lt;5 Then<BR>
			FoundErr = True<BR>
			ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;QQ&quot; &amp;&quot;长度不能大于12个字符，也不能小于5个字符！&quot;<BR>
		Else<BR>
			user_userQQ = user_userQQ<BR>
		End If<BR>
	End If<BR>
	<BR>
	If user_userRemark=&quot;&quot; Then<BR>
		'FoundErr = True<BR>
		'ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;备注&quot; &amp;&quot;不能为空！&quot;<BR>
	Else<BR>
		If CokeShow.strLength(user_userRemark)&gt;250 Or CokeShow.strLength(user_userRemark)&lt;3 Then<BR>
			FoundErr = True<BR>
			ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;备注&quot; &amp;&quot;长度不能大于250个字符，也不能小于3个字符！&quot;<BR>
		Else<BR>
			user_userRemark = user_userRemark<BR>
		End If<BR>
	End If<BR>
	<BR>
	'数字类:<BR>
	If user_groupID=&quot;&quot; Then<BR>
		'FoundErr = True<BR>
		'ErrMsg = ErrMsg &amp;&quot;&quot;&amp; UnitName &amp;&quot;不能为空！&quot;<BR>
		user_groupID = 0<BR>
	Else<BR>
		If isNumeric(user_groupID) Then<BR>
			user_groupID = CokeShow.CokeClng(user_groupID)<BR>
		Else<BR>
			FoundErr = True<BR>
			ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;所属组&quot; &amp;&quot;参数不对！&quot;<BR>
		End If<BR>
	End If<BR>
	<BR>
	If user_userSex=&quot;&quot; Then<BR>
		'FoundErr = True<BR>
		'ErrMsg = ErrMsg &amp;&quot;&quot;&amp; UnitName &amp;&quot;不能为空！&quot;<BR>
		user_userSex = 0<BR>
	Else<BR>
		If isNumeric(user_userSex) Then<BR>
			user_userSex = CokeShow.CokeClng(user_userSex)<BR>
			If (user_userSex)&gt;2 Then<BR>
				FoundErr = True<BR>
				ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;性别&quot; &amp;&quot;参数不对!！&quot;<BR>
			End If<BR>
			<BR>
		Else<BR>
			FoundErr = True<BR>
			ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;性别&quot; &amp;&quot;参数不对！&quot;<BR>
		End If<BR>
	End If<BR>
	<BR>
	'拦截错误！<BR>
	If FoundErr=True Then Exit Sub<BR>
'	Dim StrSql,iduType,iduName,iduFields,iduWhere<BR>
'	iduType=&quot;3&quot; : iduName=CurrentTableName : iduFields=&quot;&quot;		'==========begin===========<BR>
'	iduFields = iduFields &amp; &quot;id{=}&quot; 	&amp; id 		&amp; &quot;{,}&quot;<BR>
'	iduFields = iduFields &amp; &quot;myname{=}&quot; &amp; myname 	&amp; &quot;{,}&quot;<BR>
'	iduFields = iduFields &amp; &quot;mytel{=}&quot; 	&amp; mytel 	&amp; &quot;{,}&quot;<BR>
'	iduFields = iduFields &amp; &quot;remark{=}&quot; &amp; remark 	&amp; &quot;{,}&quot;<BR>
'	iduWhere = &quot;&quot; <BR>
'	StrSql = StrSql &amp; &quot;{|}&quot; &amp; IDU(Cstr(iduType), Cstr(iduName), Cstr(iduFields), Cstr(iduWhere))'======end=======<BR>
'	'response.Write postSQL(Cstr(StrSql))	'回滚用法<BR>
'	StrSql = postSQL(Cstr(StrSql))	'回滚用法<BR>
'	If StrSql=&quot;success&quot; Then<BR>
'		response.Write &quot;success&quot;<BR>
'		response.End()<BR>
'	Else<BR>
'		response.Write StrSql<BR>
'		response.End()	<BR>
'	End If<BR>
	<BR>
	Set RS = Server.CreateObject(&quot;Adodb.RecordSet&quot;)<BR>
	sql = &quot;SELECT * FROM &quot;&amp; CurrentTableName &amp;&quot; WHERE 0=0 AND user_userIsDeleted=0 AND user_userID=&quot;&amp; intID<BR>
	'RS.Open sql,CONN,1,3<BR>
	RS.Open sql,ConnStr,adOpenKeyset,adLockOptimistic,adCmdText<BR>
	<BR>
	'拦截此记录的异常情况.<BR>
	If RS.Bof And RS.Eof Then<BR>
		FoundErr = True<BR>
		ErrMsg = ErrMsg &amp; &quot;找不到指定的&quot;&amp; UnitName &amp;&quot;！&quot;<BR>
		Exit Sub<BR>
	End If<BR>
		RS(&quot;user_userUsername&quot;)	= user_userUsername<BR>
		If Len(user_userPassword)&gt;3 Then<BR>
			RS(&quot;user_userPassword&quot;)	= md5(user_userPassword)<BR>
		End If<BR>
		RS(&quot;user_groupID&quot;)		= user_groupID<BR>
		RS(&quot;user_userName&quot;)		= user_userName<BR>
		RS(&quot;user_userSex&quot;)		= user_userSex<BR>
		RS(&quot;user_userTel&quot;)		= user_userTel<BR>
		RS(&quot;user_userEmail&quot;)	= user_userEmail<BR>
		RS(&quot;user_userQQ&quot;)		= user_userQQ<BR>
		RS(&quot;user_userRemark&quot;)	= user_userRemark<BR>
		<BR>
		If Len(user_userPassword)&gt;3 Then<BR>
			RS(&quot;user_userExpiredTime&quot;)		= Now()+7		'只要更新帐号，就给予延长7天的有效期期限！<BR>
		End If<BR>
		'RS(&quot;user_userLastloginTime&quot;)	= remark<BR>
		'RS(&quot;user_userLastloginIP&quot;)		= remark<BR>
		'RS(&quot;user_userAddTime&quot;)			= remark<BR>
		'RS(&quot;user_userIsDeleted&quot;)		= remark<BR>
		'RS(&quot;user_userCookiesKey&quot;)		= remark<BR>
	RS.Update<BR>
	'获取新增的ID：<BR>
	newID	= RS(&quot;user_userID&quot;)<BR>
	newNAME	= RS(&quot;user_userName&quot;)<BR>
	RS.Close<BR>
	Set RS = Nothing<BR>
	<BR>
	'记入日志.<BR>
	Call CokeShow.AddLog(0, CurrentTableName, newID, &quot;大后台编辑操作：成功编辑了ID为&quot;&amp; newID &amp;&quot;(&quot;&amp; newNAME &amp;&quot;)的&quot;&amp; UnitName &amp;&quot;！&quot;)<BR>
	Response.Write &quot;success&quot;<BR>
End Sub<BR>
<BR>
Sub Delete()<BR>
	Dim intID<BR>
	Dim newID,newNAME,i_DeleteSub<BR>
	Dim oneFormItem<BR>
	'获取Form表单参数：<BR>
	For Each oneFormItem In Request.Form<BR>
		Execute(&quot;Dim &quot;&amp; oneFormItem)<BR>
		Execute(&quot;&quot;&amp; oneFormItem &amp;&quot; = CokeShow.filtPublicRequest(Request.Form(&quot;&quot;&quot;&amp; oneFormItem &amp;&quot;&quot;&quot;))&quot;)<BR>
	Next<BR>
	'测试：<BR>
		'response.Write myname &amp;&quot;&lt;br /&gt;&quot;<BR>
		'response.Write mytel &amp;&quot;&lt;br /&gt;&quot;<BR>
		'response.Write remark &amp;&quot;&lt;br /&gt;&quot;<BR>
		'response.Write Action &amp;&quot;&lt;br /&gt;&quot;<BR>
		'Response.End()<BR>
	<BR>
	intID = id		'检测 id 参数.<BR>
	If intID=&quot;&quot; Then<BR>
		FoundErr = True<BR>
		ErrMsg = ErrMsg &amp; &quot;请指定要删除的&quot;&amp; UnitName &amp;&quot;&quot;<BR>
	End If<BR>
	If isNumeric(intID) Then<BR>
		If intID=0 Then<BR>
			FoundErr = True<BR>
			ErrMsg = ErrMsg &amp; &quot;请指定要删除的&quot;&amp; UnitName &amp;&quot;&quot;<BR>
		End If<BR>
	End If<BR>
	<BR>
	'拦截错误！<BR>
	If FoundErr=True Then Exit Sub<BR>
	<BR>
	If Instr(intID,&quot;,&quot;)&gt;0 Then<BR>
		'intID = Split(intID,&quot;,&quot;)<BR>
		For i_DeleteSub=0 To Ubound( Split(intID,&quot;,&quot;) )<BR>
			DeleteOne( Split(intID,&quot;,&quot;)(i_DeleteSub) )<BR>
		Next<BR>
	ElseIf isNumeric(intID) Then<BR>
		DeleteOne(intID)<BR>
	Else<BR>
		FoundErr = True<BR>
		ErrMsg = ErrMsg &amp; &quot;请指定要删除的&quot;&amp; UnitName &amp;&quot;&quot;<BR>
		Exit Sub<BR>
	End If<BR>
	<BR>
	'记入日志.<BR>
	Call CokeShow.AddLog(0, CurrentTableName, intID, &quot;大后台删除操作：成功完成了批量删除ID为&quot;&amp; intID &amp;&quot;的&quot;&amp; UnitName &amp;&quot;！&quot;)<BR>
	Response.Write &quot;success&quot;<BR>
End Sub<BR>
<BR>
Sub DeleteOne(intID)<BR>
	Dim RS_Del<BR>
	intID = CokeShow.CokeClng(intID)<BR>
	If intID=0 Then Exit Sub'不理会0的ID的记录；（0一般仅仅是用作填充的）<BR>
	<BR>
	Set RS = Server.CreateObject(&quot;ADODB.RecordSet&quot;)<BR>
	sql = &quot;SELECT * FROM &quot;&amp; CurrentTableName &amp;&quot; WHERE 0=0 AND user_userIsDeleted=0 AND user_userID=&quot;&amp; intID<BR>
	'RS.Open sql,Conn,3,1<BR>
	RS.Open sql,ConnStr,adOpenForwardOnly,adLockReadOnly,adCmdText<BR>
	If Not RS.Eof Then<BR>
		'//CONN.Execute(&quot;DELETE FROM &quot;&amp; CurrentTableName &amp;&quot; WHERE id=&quot;&amp; intID)<BR>
		Set RS_Del = Server.CreateObject(&quot;ADODB.RecordSet&quot;)<BR>
		sql = &quot;UPDATE &quot;&amp; CurrentTableName &amp;&quot; SET user_userIsDeleted=1 WHERE 0=0 AND user_userIsDeleted=0 AND user_userID=&quot;&amp; intID<BR>
		RS_Del.Open sql,ConnStr,adOpenStatic,adLockReadOnly,adCmdText<BR>
		'RS_Del.Close<BR>
		'记入日志.<BR>
		'Call CokeShow.AddLog(1, CurrentTableName, intID, &quot;删除操作：成功删除了ID为&quot;&amp; intID &amp;&quot;的&quot;&amp; UnitName &amp;&quot;！&quot;)<BR>
	Else<BR>
		'找不着记录，则<BR>
		'记入日志.<BR>
		'Call CokeShow.AddLog(1, CurrentTableName, intID, &quot;删除操作：删除ID为&quot;&amp; intID &amp;&quot;的&quot;&amp; UnitName &amp;&quot;失败！此记录有可能早已丢失！&quot;)<BR>
		FoundErr = True<BR>
		ErrMsg = ErrMsg &amp; &quot;记录ID为&quot;&amp; intID &amp;&quot;的&quot;&amp; UnitName &amp;&quot;删除操作没成功，此记录有可能早已丢失！&quot;<BR>
		Exit Sub<BR>
	End If<BR>
End Sub<BR>
%&gt;

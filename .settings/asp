	&lt;%<BR>
	Dim RS,sql									'查询列表记录用的变量.<BR>
	Set RS = Server.CreateObject(&quot;Adodb.RecordSet&quot;)<BR>
	sql = &quot;SELECT TOP 1 * FROM [test_wang] WHERE 0=0 AND isDeleted=0 AND id=&quot;&amp; Clng(Request(&quot;id&quot;))<BR>
	'Response.Write sql<BR>
	'Response.End <BR>
	RS.Open sql, MM_st8588_STRING, AdOpenForwardOnly, adLockReadOnly, adCmdText<BR>
'//RS.Open sql,Conn,0,1<BR>
	%&gt;<BR>
	&lt;%<BR>
	If Not RS.EOF Then<BR>
	%&gt;<BR>
	&lt;table width=&quot;100%&quot; border=&quot;0&quot; cellspacing=&quot;2&quot; cellpadding=&quot;0&quot; summary=&quot;修改记录.&quot; id=&quot;table2&quot;&gt;<BR>
      &lt;caption&gt;<BR>
        修改记录<BR>
      &lt;/caption&gt;<BR>
      &lt;tr&gt;<BR>
        &lt;th colspan=&quot;2&quot; scope=&quot;col&quot;&gt;E&lt;/th&gt;<BR>
      &lt;/tr&gt;<BR>
      &lt;tr&gt;<BR>
        &lt;td width=&quot;24%&quot;&gt;&lt;div align=&quot;right&quot;&gt;名字：&lt;/div&gt;&lt;/td&gt;<BR>
        &lt;td width=&quot;76%&quot; align=&quot;left&quot;&gt;<BR>
			&lt;input type=&quot;text&quot; name=&quot;myname&quot; id=&quot;myname&quot; value=&quot;&lt;% =RS(&quot;myname&quot;) %&gt;&quot; /&gt;<BR>
        &lt;/td&gt;<BR>
      &lt;/tr&gt;<BR>
      &lt;tr&gt;<BR>
        &lt;td width=&quot;24%&quot;&gt;&lt;div align=&quot;right&quot;&gt;电话：&lt;/div&gt;&lt;/td&gt;<BR>
        &lt;td width=&quot;76%&quot; align=&quot;left&quot;&gt;<BR>
			&lt;input type=&quot;text&quot; name=&quot;mytel&quot; id=&quot;mytel&quot; value=&quot;&lt;% =RS(&quot;mytel&quot;) %&gt;&quot; /&gt;<BR>
        &lt;/td&gt;<BR>
      &lt;/tr&gt;<BR>
      &lt;tr&gt;<BR>
        &lt;td width=&quot;24%&quot;&gt;&lt;div align=&quot;right&quot;&gt;备注：&lt;/div&gt;&lt;/td&gt;<BR>
        &lt;td width=&quot;76%&quot; align=&quot;left&quot;&gt;<BR>
        	&lt;textarea name=&quot;remark&quot; id=&quot;remark&quot; cols=&quot;45&quot; rows=&quot;5&quot;&gt;&lt;% =RS(&quot;remark&quot;) %&gt;&lt;/textarea&gt;<BR>
        &lt;/td&gt;<BR>
      &lt;/tr&gt;<BR>
      &lt;tr&gt;<BR>
        &lt;td colspan=&quot;2&quot;&gt;<BR>
       	&lt;div align=&quot;center&quot;&gt;<BR>
       	  &lt;input type=&quot;button&quot; name=&quot;modifybtn&quot; id=&quot;modifybtn&quot; value=&quot;修改记录&quot; style=&quot;padding:10px 20px;&quot; /&gt;<BR>
          <BR>
          &lt;input type=&quot;hidden&quot; name=&quot;id&quot; id=&quot;id&quot; value=&quot;&lt;% =RS(&quot;id&quot;) %&gt;&quot; /&gt;<BR>
   	    &lt;/div&gt;&lt;/td&gt;<BR>
      &lt;/tr&gt;<BR>
    &lt;/table&gt;<BR>
    <BR>
    &lt;!--公共修改函数--&gt;<BR>
    <BR>
    &lt;%<BR>
	Else<BR>
	%&gt;<BR>
    	&lt;font color=&quot;#FF0000&quot;&gt;No Records.&lt;/font&gt;<BR>
        <BR>
	&lt;%<BR>
	End If<BR>
	RS.Close<BR>
	Set RS = Nothing<BR>
	%&gt;

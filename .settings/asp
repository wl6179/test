'		str=re.Replace(str,&quot; &quot;)<BR>
'		nohtml=str<BR>
'		Set re=Nothing<BR>
'	End Function<BR>
	<BR>
	Public Function nohtmlII(str)	'WL期待中...<BR>
		Dim re<BR>
		Set re=new RegExp<BR>
		  re.IgnoreCase=True	'忽略大小写true<BR>
		  re.Global=True		'全部匹配true<BR>
		  re.Pattern=(&quot;&lt;[^&gt;]*&gt;\s*|&amp;nbsp;&quot;)	'模式表达式<BR>
		nohtmlII=re.replace(str,&quot;&quot;)<BR>
		Set re=Nothing<BR>
	End Function<BR>

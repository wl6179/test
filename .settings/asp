CREATE TABLE [eBusiness].[system_iplog](<BR>
	[IPLog_ID] [int] IDENTITY(1,1) NOT NULL,<BR>
	[IPLog_SiteID] [int] NOT NULL CONSTRAINT [DF__system_ip__IPLog__1B0907CE]  DEFAULT ((0)),<BR>
	[IPLog_Type] [nvarchar](50) COLLATE Chinese_PRC_CI_AS NOT NULL CONSTRAINT [DF__system_ip__IPLog__1BFD2C07]  DEFAULT (''),<BR>
	[IPLog_TypeID] [nvarchar](250) COLLATE Chinese_PRC_CI_AS NOT NULL CONSTRAINT [DF__system_ip__IPLog__1CF15040]  DEFAULT (''),<BR>
	[IPLog_Info] [nvarchar](250) COLLATE Chinese_PRC_CI_AS NOT NULL CONSTRAINT [DF__system_ip__IPLog__1DE57479]  DEFAULT (''),<BR>
	[IPLog_Addr] [nvarchar](16) COLLATE Chinese_PRC_CI_AS NOT NULL CONSTRAINT [DF__system_ip__IPLog__1ED998B2]  DEFAULT (''),<BR>
	[IPLog_Area] [nvarchar](50) COLLATE Chinese_PRC_CI_AS NOT NULL CONSTRAINT [DF__system_ip__IPLog__1FCDBCEB]  DEFAULT (''),<BR>
	[IPLog_AdminID] [int] NOT NULL CONSTRAINT [DF__system_ip__IPLog__20C1E124]  DEFAULT ((0)),<BR>
	[IPLog_AdminName] [nvarchar](50) COLLATE Chinese_PRC_CI_AS NOT NULL CONSTRAINT [DF__system_ip__IPLog__21B6055D]  DEFAULT (''),<BR>
	[IPLog_AdminRemark] [nvarchar](250) COLLATE Chinese_PRC_CI_AS NOT NULL CONSTRAINT [DF__system_ip__IPLog__22AA2996]  DEFAULT (''),<BR>
	[IPLog_ClientAllInfo] [ntext] COLLATE Chinese_PRC_CI_AS NOT NULL CONSTRAINT [DF_8588_IPLog_IPLog_ClientAllInfo]  DEFAULT (''),<BR>
	[IPLog_AddDate] [smalldatetime] NOT NULL CONSTRAINT [DF_8588_IPLog_IPLog_AddDate]  DEFAULT (getdate()),<BR>
 CONSTRAINT [PK_8588_IPLog] PRIMARY KEY CLUSTERED <BR>
(<BR>
	[IPLog_ID] ASC<BR>
)WITH (IGNORE_DUP_KEY = OFF) ON [PRIMARY]<BR>
) ON [PRIMARY] TEXTIMAGE_ON [PRIMARY]<BR>


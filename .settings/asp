            	&lt;option value=&quot;0&quot; &lt;% If RS(&quot;user_userSex&quot;)=0 Then Response.Write &quot;selected&quot; %&gt;&gt;选择性别&lt;/option&gt;<BR>
                &lt;option value=&quot;1&quot; &lt;% If RS(&quot;user_userSex&quot;)=1 Then Response.Write &quot;selected&quot; %&gt;&gt;男&lt;/option&gt;<BR>

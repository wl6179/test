<DT>公共js打开窗口功能-批量删除提交调用函数js（批量）</DT>
<!--property:icon_internal=blank-->
<!--property:syntax_highlight=vbnet-->
<!--property:date_created=1364549833-->
<!--property:date_modified=1386919729-->
<!--property:expanded-->
<DD>
    &lt;!--批量删除函数--&gt;<BR>
    &lt;script type=&quot;text/javascript&quot;&gt;<BR>
		$(document).ready(function(){<BR>
			$(&quot;#All_ExecButton&quot;).click(function(){<BR>
				if (confirm('确定要批量执行吗？')) {<BR>
					//go.<BR>
				} else {<BR>
					//阻拦操作：<BR>
					return false;<BR>
				}<BR>
				var IDstr = &quot;&quot;;  <BR>
				$(&quot;[name='id'][checked]&quot;).each(function(){<BR>
					IDstr += $(this).val()+&quot;,&quot;;<BR>
					//alert($(this).val());<BR>
				})<BR>
				IDstr += &quot;0&quot;;	//ID.<BR>
				$(&quot;#All_ExecButton&quot;).toggle();		//提交时，隐藏提交按钮；<BR>
				$.ajax({url:&quot;me.Post.asp?Action=&quot; + escape($(&quot;#All_Action&quot;).val()),type:&quot;POST&quot;,data:&quot;&quot;<BR>
					+ &quot;Chris=&quot;	//操作.<BR>
					+ &quot;&amp;id=&quot;+ escape(IDstr)	//ID.<BR>
					+ &quot;&quot;,success: function(msg){<BR>
					if (msg==&quot;success&quot;){<BR>
						alert(&quot;&quot; + &quot;批量执行成功&quot; + &quot;&quot;);<BR>
						parent.location.reload();		//刷新页面<BR>
					}else{<BR>
						alert( &quot;报警：&quot; + msg + &quot;&quot; );<BR>
						$(&quot;#All_ExecButton&quot;).toggle();<BR>
						//document.write(msg)<BR>
					}},error: function(){alert( &quot;&quot; + &quot;访问失败&quot; + &quot;&quot; );}});	<BR>
			});<BR>
		 });<BR>
		function CheckAll() {<BR>
		   $(&quot;[name='id']&quot;).each(function(){<BR>
				if ($(this).attr(&quot;checked&quot;)) {<BR>
					$(this).removeAttr(&quot;checked&quot;);<BR>
				} else {<BR>
					$(this).attr(&quot;checked&quot;,'true');<BR>
				}<BR>
			})<BR>
		}<BR>
    &lt;/script&gt;
</DD>

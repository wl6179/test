	'错误提示.<BR>
	Public Sub AlertErrMsg_general(errmsg)<BR>
		<BR>
		Response.Write &quot;&lt;script&gt;alert('&quot;&amp; ErrMsg &amp;&quot;'); history.go(-1);&lt;/script&gt;&quot;<BR>
		Response.End()<BR>
		Exit Sub<BR>
		<BR>
    End Sub<BR>
	<BR>
	'错误提示.[专用于前台显示的，例如登录错误提示]<BR>
	Public Sub AlertErrMsg_foreground(errmsg)<BR>
%&gt;<BR>
		&lt;style type=&quot;text/css&quot;&gt;<BR>

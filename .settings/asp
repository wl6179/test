    &lt;%<BR>
	Dim RS,sql									'查询列表记录用的变量.<BR>
	Set RS = Server.CreateObject(&quot;Adodb.RecordSet&quot;)<BR>
	sql = &quot;SELECT TOP 1 * FROM [user_user] WHERE 0=0 AND user_userIsDeleted=0 AND user_userID=&quot;&amp; Clng(Request(&quot;id&quot;))<BR>
	'Response.Write sql<BR>
	'Response.End <BR>
	RS.Open sql, ConnStr, AdOpenForwardOnly, adLockReadOnly, adCmdText<BR>
'//RS.Open sql,Conn,0,1<BR>
	%&gt;<BR>
	&lt;%<BR>
	If Not RS.EOF Then<BR>
	%&gt;<BR>

<DT>master_user_user.asp</DT>
<!--property:icon_internal=blank-->
<!--property:syntax_highlight=vbnet-->
<!--property:date_created=1364549833-->
<!--property:date_modified=1386920448-->
<DD>
&lt;!--#include file=&quot;../inc/conn.asp&quot; --&gt;<BR>
&lt;%<BR>
'变量定义区.<BR>
'(用来存储对象的变量，用全大写!)<BR>
Const maxPerPage = 20							'当前模块分页设置.<BR>
Dim CurrentPageNow,TitleName,UnitName<BR>
CurrentPageNow 	= &quot;master_user_user.asp&quot;		'当前页.<BR>
TitleName 		= &quot;管理人员管理&quot;				'此模块管理页的名字.<BR>
UnitName 		= &quot;管理人员&quot;					'此模块涉及记录的元素名.<BR>
'自定义设置.<BR>
'本地设置.<BR>
Dim CurrentTableName<BR>
CurrentTableName 	= &quot;[user_user]&quot;		'此模块涉及的[表]名.<BR>
%&gt;<BR>
&lt;%<BR>
Dim totalPut,totalPages,currentPage,i_currRecordNumCusor		'分页用的控制变量.<BR>
Dim FoundErr,ErrMsg							'控制错误流程用的控制变量.<BR>
Dim strFileName								'构建查询字符串用的控制变量.<BR>
Dim ExecuteSearch,Keyword,TypeSearch,Action	'构建查询字符串以及流程控制用的控制变量.<BR>
Dim strGuide		'导航文字.<BR>
Dim RS,sql<BR>
<BR>
currentPage		= CokeShow.filtRequest(Request(&quot;Page&quot;))<BR>
ExecuteSearch	= CokeShow.filtRequest(Request(&quot;ExecuteSearch&quot;))<BR>
Keyword			= CokeShow.filtRequest(Request(&quot;Keyword&quot;))<BR>
TypeSearch		= CokeShow.filtRequest(Request(&quot;TypeSearch&quot;))<BR>
Action			= CokeShow.filtRequest(Request(&quot;Action&quot;))<BR>
<BR>
'处理查询执行 控制变量<BR>
If ExecuteSearch=&quot;&quot; Then<BR>
	ExecuteSearch=0<BR>
Else<BR>
	If isNumeric(ExecuteSearch) Then ExecuteSearch=CokeShow.CokeClng(ExecuteSearch) Else ExecuteSearch=0<BR>
End If<BR>
'构造查询字符串<BR>
strFileName=CurrentPageNow &amp;&quot;?ExecuteSearch=&quot;&amp; ExecuteSearch<BR>
If Keyword&lt;&gt;&quot;&quot; Then<BR>
	strFileName=strFileName&amp;&quot;&amp;Keyword=&quot;&amp; Keyword<BR>
End If<BR>
If TypeSearch&lt;&gt;&quot;&quot; Then<BR>
	strFileName=strFileName&amp;&quot;&amp;TypeSearch=&quot;&amp; TypeSearch<BR>
End If<BR>
'处理当前页码的控制变量，通过获取到的传值获取，默认为第一页1.<BR>
If currentPage&lt;&gt;&quot;&quot; Then<BR>
    If isNumeric(currentPage) Then currentPage=CokeShow.CokeClng(currentPage) Else currentPage=1<BR>
Else<BR>
	currentPage=1<BR>
End If<BR>
%&gt;<BR>
<BR>
&lt;!DOCTYPE html&gt;<BR>
&lt;html&gt;<BR>
&lt;head&gt;<BR>
    &lt;meta http-equiv=&quot;Content-Type&quot; content=&quot;text/html; charset=gb2312&quot; /&gt;<BR>
&lt;title&gt;管理人员管理测试中心&lt;/title&gt;<BR>
    &lt;meta name=&quot;Keywords&quot; content=&quot;管理人员管理测试中心&quot; /&gt;<BR>
    &lt;meta name=&quot;Description&quot; content=&quot;管理人员管理测试中心&quot; /&gt;<BR>
    &lt;meta name=&quot;robots&quot; content=&quot;noindex, nofollow&quot; /&gt;<BR>
    &lt;meta name=&quot;googlebot&quot; content=&quot;noindex, nofollow&quot; /&gt;<BR>
    &lt;!--#include virtual=&quot;/public/js/j.htm&quot; --&gt;<BR>
&lt;/head&gt;<BR>
&lt;body&gt;<BR>
	<BR>
	&lt;%<BR>
	'可替换成分页03处（可以插入分页模块03）：<BR>
	Select Case ExecuteSearch<BR>
		Case 0<BR>
			sql=&quot;SELECT TOP 500 * FROM &quot;&amp; CurrentTableName &amp;&quot; WHERE 0=0 AND user_userIsDeleted=0 &quot;&amp;_<BR>
				&quot;ORDER BY user_userID ASC&quot;<BR>
			strGuide = strGuide &amp; &quot;所有&quot;&amp; UnitName<BR>
		Case 1<BR>
			sql=&quot;SELECT TOP 500 * FROM &quot;&amp; CurrentTableName &amp;&quot; WHERE 0=0 AND isDeleted=0 AND ClassID=000 ORDER BY 0 DESC&quot;<BR>
			strGuide=strGuide &amp; &quot;所属库分类&quot;&amp; UnitName<BR>
		<BR>
		Case 10<BR>
			If Keyword=&quot;&quot; Then<BR>
				sql=&quot;SELECT TOP 500 * FROM &quot;&amp; CurrentTableName &amp;&quot; WHERE 0=0 AND isDeleted=0 ORDER BY 0 DESC&quot;<BR>
				strGuide=strGuide &amp; &quot;所有&quot;&amp; UnitName<BR>
			Else<BR>
				Select Case TypeSearch<BR>
					'查询数字参数：<BR>
					Case &quot;id&quot;<BR>
						If IsNumeric(Keyword)=False Then<BR>
							FoundErr = True<BR>
							ErrMsg = ErrMsg &amp; UnitName &amp;&quot;ID必须是整数！&quot;<BR>
						Else<BR>
							sql = &quot;SELECT TOP 500 * FROM &quot;&amp; CurrentTableName &amp;&quot; WHERE 0=0 AND isDeleted=0 AND id=&quot;&amp; CokeShow.CokeClng(Keyword)<BR>
							strGuide = strGuide &amp; UnitName &amp;&quot;ID等于&lt;font color=red&gt; &quot; &amp; CokeShow.CokeClng(Keyword) &amp; &quot; &lt;/font&gt;的&quot;&amp; UnitName<BR>
						End If<BR>
					'查询字符参数：<BR>
					Case &quot;cnname&quot;<BR>
						sql = &quot;SELECT TOP 500 * FROM &quot;&amp; CurrentTableName &amp;&quot; WHERE 0=0 AND isDeleted=0 AND cnname LIKE '%&quot;&amp; Keyword &amp;&quot;%' ORDER BY 0 DESC&quot;<BR>
						strGuide = strGuide &amp; &quot;中文名中含有“ &lt;font color=red&gt;&quot; &amp; Keyword &amp; &quot;&lt;/font&gt; ”的&quot;&amp; UnitName<BR>
					'查询其它：<BR>
					Case &quot;username&quot;<BR>
						sql = &quot;SELECT TOP 500 * FROM &quot;&amp; CurrentTableName &amp;&quot; WHERE 0=0 AND isDeleted=0 AND username LIKE '%&quot;&amp; Keyword &amp;&quot;%' ORDER BY 0 DESC&quot;<BR>
						strGuide = strGuide &amp; &quot;帐号中含有“ &lt;font color=red&gt;&quot; &amp; Keyword &amp; &quot;&lt;/font&gt; ”的&quot;&amp; UnitName<BR>
					<BR>
				End Select<BR>
				<BR>
			End If<BR>
			<BR>
			<BR>
		Case Else<BR>
			FoundErr = True<BR>
			ErrMsg = ErrMsg &amp;&quot;错误的参数！&quot;<BR>
		<BR>
	End Select<BR>
<BR>
	'拦截错误.<BR>
	If FoundErr=True Then<BR>
		Response.Write ErrMsg<BR>
		Response.End()<BR>
	End If<BR>
<BR>
    'Dim RS,sql									'查询列表记录用的变量.<BR>
    Set RS = Server.CreateObject(&quot;Adodb.RecordSet&quot;)<BR>
'可替换成分页03处（可以插入分页模块03）：<BR>
    'sql = &quot;SELECT TOP 500 id,myname,mytel,remark FROM [test_wang] WHERE 0=0 AND isDeleted=0 ORDER BY id DESC&quot;<BR>
    'Response.Write sql<BR>
    'Response.End <BR>
    RS.Open sql,ConnStr,adOpenKeyset,adLockReadOnly,adCmdText<BR>
'RS.Open sql,Conn,1,1<BR>
    %&gt;<BR>
    &lt;a href=&quot;master_user_user.Add.asp?KeepThis=true&amp;TB_iframe=true&amp;height=550&amp;width=600&amp;modal=false&quot; title=&quot;添加新记录&quot; class=&quot;thickbox&quot;&gt;+添加&lt;/a&gt;<BR>
    <BR>
    &lt;hr /&gt;<BR>
    <BR>
    &lt;table width=&quot;100%&quot; border=&quot;0&quot; cellspacing=&quot;2&quot; cellpadding=&quot;0&quot; summary=&quot;摘要哈&quot; id=&quot;table2&quot;&gt;<BR>
    &lt;!--List--&gt;<BR>
      &lt;caption&gt;<BR>
        &lt;% =TitleName %&gt;列表<BR>
      &lt;/caption&gt;<BR>
      &lt;tr&gt;<BR>
        &lt;th scope=&quot;col&quot;&gt;&lt;input type=&quot;checkbox&quot; name=&quot;All_CheckAll&quot; id=&quot;All_CheckAll&quot; value=&quot;All_CheckAll&quot; onClick=&quot;CheckAll();&quot; /&gt;&lt;label for=&quot;All_CheckAll&quot;&gt;全/反选&lt;/label&gt;&amp;nbsp;&lt;/th&gt;<BR>
        &lt;th scope=&quot;col&quot;&gt;ID&amp;nbsp;&lt;/th&gt;<BR>
        &lt;th scope=&quot;col&quot;&gt;名字&amp;nbsp;&lt;/th&gt;<BR>
        &lt;th scope=&quot;col&quot;&gt;电话&amp;nbsp;&lt;/th&gt;<BR>
        &lt;th scope=&quot;col&quot;&gt;操作&amp;nbsp;&lt;/th&gt;<BR>
      &lt;/tr&gt;<BR>
        &lt;!--List--&gt;<BR>
    &lt;%<BR>
	'可插入分页05：<BR>
	If RS.Eof And RS.Bof Then<BR>
		strGuide = strGuide &amp;&quot; &amp;#187; 共找到 &lt;font color=red&gt;0&lt;/font&gt; 个&quot;&amp; UnitName<BR>
		'//Call showMain<BR>
	Else<BR>
    	'//Dim RS_temp,sql_temp							'查询列表记录用的变量.<BR>
    	'//Set RS_temp = Server.CreateObject(&quot;Adodb.RecordSet&quot;)<BR>
		'//sql_temp = Replace(sql,&quot;SELECT TOP 500 * &quot;,&quot;SELECT COUNT(*) &quot;)<BR>
		'//sql_temp = Replace(sql_temp, Mid(sql_temp,Instr(sql_temp,&quot;ORDER BY&quot;),Len(sql_temp)), &quot;&quot;)<BR>
		'//Response.Write sql_temp<BR>
		'//Response.End()<BR>
		'//RS_temp.Open sql_temp,ConnStr,AdOpenForwardOnly,adLockReadOnly,adCmdText<BR>
		totalPut = RS.RecordCount		'记录总数.<BR>
		'//RS_temp.Close<BR>
		'//Set RS_temp = Nothing<BR>
		'Response.Write &quot;&lt;br /&gt;totle:&quot;&amp; totalPut<BR>
		'//Response.End()<BR>
		<BR>
		strGuide = strGuide &amp;&quot; &amp;#187; 共找到 &lt;font color=red&gt;&quot;&amp; totalPut &amp;&quot;&lt;/font&gt; 个&quot;&amp; UnitName<BR>
		<BR>
		'处理页码<BR>
		If currentPage&lt;1 Then<BR>
       		currentPage = 1<BR>
    	End If<BR>
		'如果传递过来的Page当前页值很大，超过了应有的页数时，进行处理.<BR>
    	If (currentPage-1) * maxPerPage &gt; totalPut Then<BR>
	   		If (totalPut Mod maxPerPage)=0 Then<BR>
	     		'如果整好够页数，赋予当前页最大页.<BR>
				currentPage = totalPut \ maxPerPage<BR>
		  	Else<BR>
		      	'如果不整好，最有一页只有零散几条记录（不丰满的多余页），赋予当前页最大页.（不能整除情况下计算）<BR>
				currentPage = totalPut \ maxPerPage + 1<BR>
	   		End If<BR>
    	End If<BR>
	    If currentPage=1 Then<BR>
        	'//Call showMain<BR>
   	 	Else<BR>
   	     	'如果传递过来的Page当前页值不大，在应有的页数范围之内时，理应(currentPage-1) * maxPerPage &lt; totalPut，此时进行一些处理.<BR>
			if (currentPage-1) * maxPerPage &lt; totalPut then<BR>
         	   	'指针指到(currentPage-1)页（前一页）的最后一个记录处.<BR>
				RS.Move  (currentPage-1) * maxPerPage<BR>
				'RS.BookMark？<BR>
         		Dim bookMark<BR>
           		bookMark = RS.BookMark<BR>
            	'//Call showMain<BR>
        	else<BR>
			'如果传递过来的Page当前页值很大，超过了应有的页数时.打开第一页.<BR>
	        	currentPage = 1<BR>
           		'//Call showMain<BR>
	    	end if<BR>
		End If<BR>
	End If<BR>
	%&gt;<BR>
	&lt;%<BR>
    Do While Not RS.EOF<BR>
    %&gt;<BR>
    <BR>
    <BR>
	<BR>
      &lt;tr&gt;<BR>
        &lt;td&gt;&lt;input type=&quot;checkbox&quot; name=&quot;id&quot; id=&quot;id&lt;%= RS(0) %&gt;&quot; value=&quot;&lt;%= RS(0) %&gt;&quot; /&gt;&amp;nbsp;&lt;/td&gt;<BR>
        &lt;td&gt;&lt;%= RS(0) %&gt;&amp;nbsp;&lt;/td&gt;<BR>
        &lt;td&gt;&lt;%= RS(1) %&gt;&amp;nbsp;&lt;/td&gt;<BR>
        &lt;td&gt;&lt;%= RS(2) %&gt;&amp;nbsp;&lt;/td&gt;<BR>
        &lt;td&gt;<BR>
        	&lt;span id=&quot;showinfo_edit&lt;%=(Rs.Fields.Item(0).Value)%&gt;&quot;&gt;&lt;a href=&quot;master_user_user.Edit.asp?id=&lt;%=(Rs.Fields.Item(0).Value)%&gt;&amp;KeepThis=true&amp;TB_iframe=true&amp;height=650&amp;width=600&amp;modal=false&quot;  title=&quot;修改 &lt;b&gt;&lt;%=(Rs.Fields.Item(&quot;myname&quot;).Value)%&gt;&lt;/b&gt; 的信息&quot; class=&quot;thickbox&quot;&gt;修改&lt;/a&gt;&lt;/span&gt;<BR>
            &amp;nbsp;&lt;a href=&quot;#&quot; onClick=&quot;javascript:if (confirm('确定要删除吗？')){deleteRS(&lt;% =(Rs.Fields.Item(0).Value) %&gt;);}else{return false;}&quot;&gt;删除&lt;/a&gt;<BR>
        &lt;/td&gt;<BR>
      &lt;/tr&gt;<BR>
      <BR>
      <BR>
      <BR>
	&lt;%<BR>
        RS.MoveNext<BR>
		'可插入分页06：<BR>
		i_currRecordNumCusor = i_currRecordNumCusor + 1<BR>
		If i_currRecordNumCusor&gt;=maxPerPage Then Exit Do<BR>
    Loop<BR>
    <BR>
    RS.Close<BR>
    Set RS = Nothing<BR>
    %&gt;<BR>
      &lt;tr&gt;<BR>
        &lt;td colspan=&quot;5&quot;&gt;<BR>
        	定义操作:<BR>
            &lt;select name=&quot;All_Action&quot; id=&quot;All_Action&quot;&gt;<BR>
            	&lt;option value=&quot;Delete&quot;&gt;删除操作&lt;/option&gt;<BR>
            &lt;/select&gt;<BR>
            &amp;nbsp;<BR>
            &lt;button type=&quot;button&quot; name=&quot;All_ExecButton&quot; id=&quot;All_ExecButton&quot;&gt;批量执行&lt;/button&gt;<BR>
        &lt;/td&gt;<BR>
      &lt;/tr&gt;<BR>
      &lt;!--可插入分页04--&gt;<BR>
      &lt;tr&gt;<BR>
        &lt;td colspan=&quot;5&quot;&gt;<BR>
        	&lt;%<BR>
			response.write CokeShow.ShowPage(strFileName,totalPut,maxPerPage,True,True,&quot;个&quot;&amp; UnitName)<BR>
			%&gt;<BR>
        &lt;/td&gt;<BR>
      &lt;/tr&gt;<BR>
    &lt;!--List--&gt;<BR>
    <BR>
    &lt;/table&gt;<BR>
	&lt;!--公共删除函数--&gt;<BR>
    &lt;script type=&quot;text/javascript&quot;&gt;<BR>
		function deleteRS(paraID)<BR>
		{<BR>
			$.ajax({url:&quot;master_user_user.Post.asp?Action=Delete&quot;,type:&quot;POST&quot;,data:&quot;&quot;<BR>
				+ &quot;Chris=&quot;	//操作.<BR>
				+ &quot;&amp;id=&quot;+ paraID<BR>
				+ &quot;&quot;,success: function(msg){<BR>
				if (msg==&quot;success&quot;){<BR>
					alert(&quot;&quot; + &quot;删除提交成功&quot; + &quot;&quot;);<BR>
					parent.location.reload();		//刷新页面<BR>
				}else{<BR>
					alert( &quot;报警：&quot; + msg + &quot;&quot; );<BR>
					//document.write(msg)<BR>
				}},error: function(){alert( &quot;&quot; + &quot;访问失败&quot; + &quot;&quot; );}});	<BR>
		}<BR>
    &lt;/script&gt;<BR>
    &lt;!--批量删除函数--&gt;<BR>
    &lt;script type=&quot;text/javascript&quot;&gt;<BR>
		$(document).ready(function(){<BR>
			$(&quot;#All_ExecButton&quot;).click(function(){<BR>
				if (confirm('确定要批量执行吗？')) {<BR>
					//go.<BR>
				} else {<BR>
					//阻拦操作：<BR>
					return false;<BR>
				}<BR>
				var IDstr = &quot;&quot;;  <BR>
				$(&quot;[name='id'][checked]&quot;).each(function(){<BR>
					IDstr += $(this).val()+&quot;,&quot;;<BR>
					//alert($(this).val());<BR>
				})<BR>
				IDstr += &quot;0&quot;;	//ID.<BR>
				$(&quot;#All_ExecButton&quot;).toggle();		//提交时，隐藏提交按钮；<BR>
				$.ajax({url:&quot;master_user_user.Post.asp?Action=&quot; + escape($(&quot;#All_Action&quot;).val()),type:&quot;POST&quot;,data:&quot;&quot;<BR>
					+ &quot;Chris=&quot;	//操作.<BR>
					+ &quot;&amp;id=&quot;+ escape(IDstr)	//ID.<BR>
					+ &quot;&quot;,success: function(msg){<BR>
					if (msg==&quot;success&quot;){<BR>
						alert(&quot;&quot; + &quot;批量执行成功&quot; + &quot;&quot;);<BR>
						parent.location.reload();		//刷新页面<BR>
					}else{<BR>
						alert( &quot;报警：&quot; + msg + &quot;&quot; );<BR>
						$(&quot;#All_ExecButton&quot;).toggle();<BR>
						//document.write(msg)<BR>
					}},error: function(){alert( &quot;&quot; + &quot;访问失败&quot; + &quot;&quot; );}});	<BR>
			});<BR>
		 });<BR>
		function CheckAll() {<BR>
		   $(&quot;[name='id']&quot;).each(function(){<BR>
				if ($(this).attr(&quot;checked&quot;)) {<BR>
					$(this).removeAttr(&quot;checked&quot;);<BR>
				} else {<BR>
					$(this).attr(&quot;checked&quot;,'true');<BR>
				}<BR>
			})<BR>
		}<BR>
    &lt;/script&gt;<BR>
    <BR>

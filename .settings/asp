&lt;%@ LANGUAGE = VBScript CodePage = 936%&gt;<BR>
&lt;%<BR>
 '*********************************************************<BR>
 ' 文    件：	inc\Conn.asp<BR>
 ' 所属项目：	8588 电商新系统<BR>
 ' 文件功能：	配置数据库连接信息及初始化数据库连接<BR>
 ' 最后更新：	2023-12-03 11:03<BR>
 ' 项目负责：	一想千开 (http://www.yes1000.com)<BR>
 '*********************************************************<BR>
<BR>
Option Explicit<BR>
Response.Buffer = True  <BR>
Response.Charset = &quot;GB2312&quot;<BR>
%&gt;<BR>
<BR>
&lt;!--#include file=&quot;360_safe3.asp&quot; --&gt;<BR>
<BR>
&lt;%<BR>
Dim Conn<BR>
Dim ConnStr<BR>
ConnStr = &quot;&quot;<BR>
'=========================SQL数据库设置=========================<BR>
Const SqlServer = &quot;127.0.0.1,1433&quot;			'数据库服务器<BR>
'Const SqlServer = &quot;CCCF-B4E493FC45&quot;		'数据库服务器<BR>
'Const SqlServer = &quot;192.168.163.188,1433&quot;	'数据库服务器<BR>
Const SqlDbName = &quot;eBusiness&quot;				'数据库名<BR>
'Const SqlUsername = &quot;sa&quot;					'用户名<BR>
'Const SqlPassword = &quot;win2000&quot;				'用户密码<BR>
Const SqlUsername = &quot;eBusiness&quot;		'用户名<BR>
Const SqlPassword = &quot;FB4A92A7E38a86&quot; 		'用户密码<BR>
<BR>
OpenDB		'只要包含此文件，就默认打开链接！~<BR>
<BR>
Sub ConnectionDB<BR>
	ConnStr = &quot;provider=SQLNCLI; UID=&quot;&amp; SqlUsername &amp;&quot;; PWD=&quot;&amp; SqlPassword &amp;&quot;; DATABASE=&quot;&amp; SqlDbName &amp;&quot;; Data Source=&quot;&amp; SqlServer &amp;&quot;;&quot;<BR>
	On Error Resume Next<BR>
	Set Conn = Server.CreateObject(&quot;ADODB.Connection&quot;)<BR>
	Conn.Open ConnStr<BR>
	If Err Then<BR>
		Err.Clear<BR>
		Set Conn = Nothing<BR>
		Response.Write &quot;数据库连出错，请检查数据库设置。&quot;<BR>
		Response.End()<BR>
	End If<BR>
End Sub<BR>
<BR>
Sub OpenDB<BR>
	If Not IsObject(Conn) Then<BR>
		ConnectionDB<BR>
	End If<BR>
End Sub<BR>
<BR>
Sub CloseDB<BR>
	If IsObject(Conn) Then<BR>
		Conn.Close<BR>
		Set Conn = Nothing<BR>
		Conn = &quot;&quot;<BR>
	End If<BR>
End Sub<BR>
%&gt;<BR>
&lt;!--#include file=&quot;../inc/md5.asp&quot; --&gt;<BR>
&lt;!-- METADATA TYPE=&quot;typelib&quot; FILE=&quot;C:\Program Files\Common Files\System\ado\msado15.dll&quot; --&gt;<BR>
&lt;!--#include file=&quot;../system/system_class.asp&quot;--&gt;<BR>
&lt;%<BR>
'增强类：<BR>
Dim CokeShow<BR>
Set CokeShow = New SystemClass<BR>
'CokeShow.Start		'调用类的Start方法，初始化类里的ReloadSetup()函数，并得到二维数组Setup<BR>
'Call CokeShow.SQLWarningSys()	'预警.<BR>
'A: <BR>
	'adOpenForwardOnly(=0) <BR>
	'只读,且当前数据记录只能向下移动<BR>
	'adOpenKeyset(=1) <BR>
	'只读,当前数据记录可自由移动<BR>
	'adOpenDynamic(=2) <BR>
	'可读写,当前数据记录可自由移动<BR>
	'adOpenStatic(=3) <BR>
	'可读写,当前数据记录可自由移动,可看到新增记录<BR>
'B:<BR>
	'adLockReadOnly(=1) <BR>
	'缺省锁定类型，记录集是只读的，不能修改记录<BR>
	'adLockPessimistic(=2) <BR>
	'悲观锁定，当修改记录时，数据提供者将尝试锁定记录以确保成功地编辑记录。只要编辑一开始，则立即锁住记录。<BR>
	'adLockOptimistic(=3) <BR>
	'乐观锁定 ，直到用Update方法提交更新记录时才锁定记录。<BR>
	'adLockBatchOptimistic(=4) <BR>
	'批量乐观锁定，允许修改多个记录，只有调用UpdateBatch方法后才锁定记录。<BR>
%&gt;
</DD>
<DT>Global.asa</DT>
<!--property:icon_internal=blank-->
<!--property:syntax_highlight=vbnet-->
<!--property:date_created=1364549833-->
<!--property:date_modified=1386920337-->
<DD>
 &lt;Script Language=&quot;VBscript&quot; Runat=&quot;Server&quot;&gt;<BR>
Sub Application_Onstart<BR>
	'Chris<BR>
	'2013-04-26<BR>
	Application(&quot;numberOnLockedMaxTryTimes&quot;)	= 10		'定义 10 【次】为(某IP的)统计超额数。<BR>
	Application(&quot;numberOnLockedTheTime&quot;)		= (5*60)	'定义 300【秒】（即 5 分钟）为登录超数限制的锁定时间。<BR>
	Application(&quot;aaa&quot;)							= &quot;aaa测试&quot;	'测试<BR>
End Sub<BR>
<BR>
Sub Application_OnEnd<BR>
	'Application_OnEnd		'当该站点的 WEB 服务器关闭时运行<BR>
End Sub<BR>
<BR>
Sub Session_OnStart<BR>
	'Chris<BR>
	'2013-04-26<BR>
	If Not isObject( Application(&quot;numberOnLockedMaxTryTimes&quot;) ) Then<BR>
		'Response.write &quot;mou a!&quot;<BR>
		Application.Lock<BR>
		Application(&quot;numberOnLockedMaxTryTimes&quot;)= 10		'定义 10 【次】为(某IP的)统计超额数。<BR>
		Application.Unlock<BR>
	End If<BR>
	If Not isObject( Application(&quot;numberOnLockedTheTime&quot;) ) Then<BR>
		Application.Lock<BR>
		Application(&quot;numberOnLockedTheTime&quot;)	= (5*60)	'定义 300【秒】（即 5 分钟）为登录超数限制的锁定时间。<BR>
		Application.Unlock<BR>
	End If<BR>
	<BR>
	'Response.Redirect(&quot;index.htm&quot;)			'只要是用户第一次登录本站，则会跳转到首页<BR>
	'一个用户访问进行记数加1<BR>
	'Application.Lock<BR>
	'Application(&quot;OnLine&quot;) = Application(&quot;OnLine&quot;) + 1<BR>
	'Application.Unlock<BR>
End Sub<BR>
<BR>
Sub Session_OnEnd<BR>
	'一个用户进程的结束，记数减1 （如果没有该事件，则执行的就是页面访问程序了。）<BR>
	'Application.Lock<BR>
	'Application(&quot;OnLine&quot;) = Application(&quot;OnLine&quot;) - 1<BR>
	'Application.Unlock<BR>
End Sub<BR>
&lt;/Script&gt;
</DD>
</DL>


          &lt;input name=&quot;loginbtn&quot; id=&quot;loginbtn&quot; type=&quot;button&quot; value=&quot;   登录   &quot; style=&quot;padding:10px 20px;&quot; /&gt;<BR>
          <BR>
          &lt;input name=&quot;it&quot; id=&quot;it&quot; type=&quot;hidden&quot; value=&quot;&lt;% =Replace(CokeShow.PassEncode( Ucase(Md5( Session(&quot;YouCanLoginID_Temp&quot;) )) ),&quot;%&quot;,&quot;&quot;) %&gt;&quot; /&gt;<BR>

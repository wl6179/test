'自动化Auto：算法处理函数专区<BR>
'*************************************************************************<BR>
	'对广告词识别其规格.<BR>
	'参数：<BR>
	'1.strParaWordName:	当前广告词的内容文字.<BR>
	'结果：返回字数规格：4、6、8、10、12、50...<BR>
	Public Function intStandard_UpWord(strParaWordName)<BR>
		Dim tmpStr<BR>
		'处理一下空格，去掉：<BR>
		tmpStr = Trim( Replace(strParaWordName,&quot; &quot;,&quot;&quot;) )<BR>
		<BR>
		If Len(tmpStr)&gt;2 And Len(tmpStr)&lt;=6 Then<BR>
			intStandard_UpWord = 4<BR>
		End If<BR>
		If Len(tmpStr)&gt;6 And Len(tmpStr)&lt;=8 Then<BR>
			intStandard_UpWord = 8<BR>
		End If<BR>
		If Len(tmpStr)&gt;8 And Len(tmpStr)&lt;=12 Then<BR>
			intStandard_UpWord = 10<BR>
		End If<BR>
<BR>
		If Len(tmpStr)&gt;12 And Len(tmpStr)&lt;=25 Then<BR>
			intStandard_UpWord = 25<BR>
		End If<BR>
		If Len(tmpStr)&gt;25 And Len(tmpStr)&lt;=200 Then<BR>
			intStandard_UpWord = 50<BR>
		End If<BR>
		<BR>
	End Function<BR>
	<BR>
	'对广告图片识别其规格.<BR>
	'参数：<BR>
	'1.floatParaBiLiZhi:	当前广告图片文件的宽高比例值.<BR>
	'结果：返回图片比例规格（ 及其比例的官方表示文字 3:2 等~ ）：&quot;1|1:1&quot;、&quot;1.5|3:2&quot;、&quot;2|2:1&quot;、&quot;3|3:1&quot;...<BR>
	Public Function float_str__Standard_UpFile(floatParaBiLiZhi)<BR>
		Dim tmpIntNumber<BR>
		tmpIntNumber=0<BR>
		<BR>
		'2种比例：<BR>
		'1.6<BR>
		If floatParaBiLiZhi&gt;0.5 And floatParaBiLiZhi&lt;=1.7 Then<BR>
			float_str__Standard_UpFile = &quot;1.6&quot;		'1.6比1<BR>
			tmpIntNumber = 1<BR>
		End If<BR>
		'2.0<BR>
		If floatParaBiLiZhi&gt;1.7 And floatParaBiLiZhi&lt;=3 Then<BR>
			float_str__Standard_UpFile = &quot;2.0&quot;		'2比1<BR>
			tmpIntNumber = 1<BR>
		End If<BR>
		<BR>
'//		'3种比例：<BR>
'//		If floatParaBiLiZhi&gt;=0.5 And floatParaBiLiZhi&lt;=1 Then<BR>
'//			float_str__Standard_UpFile = &quot;1|1:1&quot;		'1比1<BR>
'//			tmpIntNumber = 1<BR>
'//		End If<BR>
'//		If floatParaBiLiZhi&gt;1 And floatParaBiLiZhi&lt;=1.5 Then<BR>
'//			float_str__Standard_UpFile = &quot;1.5|3:2&quot;		'3比2<BR>
'//			tmpIntNumber = 1<BR>
'//		End If<BR>
'//		If floatParaBiLiZhi&gt;1.5 And floatParaBiLiZhi&lt;=2 Then<BR>
'//			float_str__Standard_UpFile = &quot;2|2:1&quot;		'2比1<BR>
'//			tmpIntNumber = 1<BR>
'//		End If<BR>
		'其它比例：<BR>
		If tmpIntNumber=0 Then<BR>
			float_str__Standard_UpFile = &quot;超出范围！&quot;		'3比1<BR>
		End If<BR>
		<BR>
	End Function<BR>
	<BR>
	<BR>
	'留言超标（百分比）算法 的情况：<BR>
	'有拦截返回 大于0 的当前广告的今日留言总量vs所需留言数 之 留言超标情况&#8212;&#8212;记录下已超标至 几% 时拦截的；<BR>
	'无拦截返回0.<BR>
	'注意：AND Book_isDiscontinue=0 条件都加上（所有统计留言数之处！）<BR>
	'0 代表通行；<BR>
	'&gt;0 代表 不是广告丢失，就是广告留言已经超标。（999！ 或者 超标时的比例值！）<BR>
	Public Function ChkTotleBook_ForADIDToday( BK_ADID )		'通过BK_ADID组织算法计算.<BR>
		ChkTotleBook_ForADIDToday = 0		'当前百分比情况不需要拦截留言.（默认 没超标）<BR>
		<BR>
		Dim sqlChk,rsChk<BR>
		OpenDb<BR>
		Dim numTotleBook_ForADIDToday<BR>
		<BR>
		'今日留言量：（临时tmpNums标记）<BR>
		'numTotleBook_ForADIDToday = Conn.Execute(&quot;SELECT COUNT(book_id) FROM [system_Count_Book] WHERE 1=1 AND DATEDIFF(d, Book_AddTime, GETDATE())=0 AND book_AdListid=&quot;&amp; BK_ADID &amp;&quot;&quot;)(0)	'统计所有的留言.<BR>
		numTotleBook_ForADIDToday = Conn.Execute(&quot;SELECT COUNT(book_id) FROM [system_Count_Book] WHERE 1=1 AND Book_sysLogic_Deleted=0 AND Book_isDiscontinue=0 AND DATEDIFF(d, Book_AddTime, GETDATE())=0 AND book_AdListid=&quot;&amp; BK_ADID &amp;&quot;&quot;)(0)<BR>
	<BR>
		sqlChk = &quot;SELECT AdList_ID,AdList_ADNeedMessageNum FROM [system_AdList] WHERE 1=1 AND AdList_sysLogic_Deleted=0 AND AdList_ID=&quot;&amp; BK_ADID<BR>
		Set rsChk = CONN.Execute(sqlChk)<BR>
	'response.Write sqlChk<BR>
	'response.End()	<BR>
		'如果无相应y_d记录，当作是未知域名来源！就将（域名）拦截【原因】是设为0.<BR>
		If rsChk.Bof And rsChk.Eof Then<BR>
			ChkTotleBook_ForADIDToday = 999		'等于999.（表示是 未知、或者可能是[已下线]广告的留言）（通行X -&gt;拦截√）<BR>
		ElseIf rsChk(&quot;AdList_ADNeedMessageNum&quot;)=0 Then<BR>
			ChkTotleBook_ForADIDToday = 0		'如果没有设置 留言数 要求，；（通行！）<BR>
		ElseIf FormatNumber( Clng(numTotleBook_ForADIDToday)/rsChk(&quot;AdList_ADNeedMessageNum&quot;), 2 )&gt;=1.0 Then		'设置为拦截！<BR>
			ChkTotleBook_ForADIDToday = FormatNumber( Clng(numTotleBook_ForADIDToday)/rsChk(&quot;AdList_ADNeedMessageNum&quot;), 2 )		'肯定大于0！（直接获得 被拦截时刻的 今日留言到达百分比数字！）（拦截！）<BR>
		Else		'设置为不拦截！<BR>
			ChkTotleBook_ForADIDToday = 0		'其它的应该是 今日留言量远未到达百分比的时刻的 留言（通行！）<BR>
		End If<BR>
		<BR>
		rsChk.Close<BR>
		Set rsChk = Nothing<BR>
		<BR>
	End Function<BR>
	<BR>
	<BR>
	Function oflink_lcasetag(imgstrng)<BR>
		Dim regEx, Match, Matches '建立变量。<BR>
		Set regEx = New RegExp '建立正则表达式。<BR>
		regEx.Pattern = &quot;$.+?$&quot; '设置模式。<BR>
		regEx.IgnoreCase = true '设置是否区分字符大小写。<BR>
		regEx.Global = True '设置全局可用性。<BR>
		Set Matches = regEx.Execute(imgstrng) '执行搜索。<BR>
		imgstrng=imgstrng<BR>
		For Each Match in Matches '遍历匹配集合。<BR>
			imgstrng=replace(imgstrng, Match.Value, lcase(Match.Value))<BR>
			imgstrng=replace(imgstrng, &quot;&lt;br&gt;&quot;, &quot;&lt;br /&gt;&quot;)<BR>
			imgstrng=replace(imgstrng, &quot;&lt;hr&gt;&quot;, &quot;&lt;hr /&gt;&quot;)<BR>
		Next<BR>
		oflink_lcasetag = imgstrng<BR>
	End Function<BR>
	<BR>
	<BR>
	<BR>
'自动化Auto：数据Show专区<BR>
'*************************************************************************<BR>
	'--------------------------------------<BR>
	'///            单图,通用			///<BR>
	'/// width：图片宽度   				///<BR>
	'/// height：图片高度   			///<BR>
	'/// alt：替换文本(无信息时生效)    ///<BR>
	'/// Standard_UpWord：使用4、6、8、10、12、更多 类型的广告词词库文字？！0代表随便任何一种类型皆可！(自动化时用)<BR>
	'--------------------------------------<BR>
	Public Sub areaPic(Summary,isPic,aLinkClassname,aLinkStyleColor,picWidth,picHeight,Standard_UpWord,isUseTemplate,ADisInSites,Standard_UpFile,paraintRndNumber)<BR>
		Randomize<BR>
		Dim intRndNumber<BR>
		intRndNumber = Int(10000*Rnd) + 1 &amp; Second(Time()) &amp; CokeShow.GetRandomizeCode()<BR>
		If paraintRndNumber&lt;&gt;&quot;&quot; Then intRndNumber=paraintRndNumber<BR>
		<BR>
		Dim rsArea<BR>
		<BR>
		If picWidth=0 Then picWidth=&quot;&quot;<BR>
		If picHeight=0 Then picHeight=&quot;&quot;<BR>
		<BR>
		OpenDb<BR>
		Set rsArea = Server.CreateObject(&quot;ADODB.RecordSet&quot;)<BR>
		'rsArea.Open &quot;Select Top 1 AdPlace_ShowTitle,AdPlace_ShowPic,AdPlace_LinkUrl,AdPlace_ShowText From [system_AdPlace] Where AdPlace_Summary = '&quot;&amp; Summary &amp;&quot;' And AdPlace_Enable = 1 AND ('&quot;&amp; date() &amp;&quot;' BETWEEN AdPlace_StartDate AND AdPlace_EndDate)&quot;,conn,1,1<BR>
		rsArea.Open &quot;SELECT TOP 1 &quot;&amp;_<BR>
			&quot;AdPlace_ShowTitle,AdPlace_ShowPic,AdPlace_LinkUrl,AdPlace_ShowText &quot;&amp;_<BR>
		&quot;FROM [system_AdPlace] ADP &quot;&amp;_<BR>
			&quot;LEFT JOIN [system_AdList] AD ON (ADP.AdPlace_AdListID = AD.AdList_ID AND AD.AdList_sysLogic_Deleted=0 ) &quot;&amp;_<BR>
		&quot;WHERE 1=1 AND AdPlace_sysLogic_Deleted=0 &quot;&amp;_<BR>
			&quot;AND AdPlace_Summary = '&quot;&amp; Summary &amp;&quot;' &quot;&amp;_<BR>
			&quot;AND AdPlace_Enable = 1 &quot;&amp;_<BR>
			&quot;/*AND AdPlace_IsMain = 1*/ &quot;&amp;_<BR>
			&quot;AND AdList_ID IS NOT NULL &quot;&amp;_<BR>
			&quot;AND AdList_EnGuestBook=1 &quot;&amp;_<BR>
			&quot;AND DATEDIFF(day,AdList_StartDate,GETDATE())&gt;=0 &quot;,Conn,1,1<BR>
			'【手动化】<BR>
			'01条件：已启用的位，可以Show出来！否则也自动下线！！！~~~<BR>
			'01条件：顺便过滤掉那些没有关联ADID的广告位（孤儿）！~<BR>
			'02条件：到期日的下午18点时，自动下线整个广告！！！~~~<BR>
			'02条件：DATEDIFF(hh, CONVERT( varchar(10), '2013-07-03 11:11:11', 120 ), GETDATE()),* 【CONVERT函数，确保尾部时间不进行计算！即被当作00:00:00来计算。】<BR>
			'03条件：开始日期当天（立刻），开始生效，并Show出具体广告位来！~【从起始日期的0点开始生效】<BR>
			'&amp;_<BR>
			'&quot;AND (   (DATEDIFF(hh, CONVERT( varchar(10), AdList_EndDate, 120 ), GETDATE())&lt;18 AND AdList_isForTimesOrBooks=0)   OR   ( AdList_ADNeedMessageNum_InAllDates&gt;(SELECT COUNT(book_id) FROM [system_Count_Book] WHERE 1=1 AND Book_sysLogic_Deleted=0 AND Book_isDiscontinue=0 AND DATEDIFF(d, AdList_StartDate, Book_AddTime)&gt;=0 AND book_AdListid=AD.AdList_ID) AND AdList_isForTimesOrBooks=1)   ) &quot;<BR>
		<BR>
		If Not rsArea.Eof Then<BR>
			<BR>
			If isUseTemplate=0 Then<BR>
			'只处理a、img的这部分（通用，随处可用！）- 若有其它标签则用HTML模板来处理！~<BR>
			%&gt;&lt;a href=&quot;&lt;% =rsArea(&quot;AdPlace_LinkUrl&quot;) %&gt;&lt;% If Instr(rsArea(&quot;AdPlace_LinkUrl&quot;),&quot;?&quot;)&gt;0 Then Response.Write &quot;&amp;&quot; Else Response.Write &quot;?&quot; %&gt;&lt;% =&quot;isP_d=&quot;&amp; isPic &amp;&quot;&amp;pW_d=&quot;&amp; picWidth &amp;&quot;&amp;pH_d=&quot;&amp; picHeight &amp;&quot;&amp;StandW_d=&quot;&amp; Standard_UpWord &amp;&quot;&amp;isUT_d=&quot;&amp; isUseTemplate &amp;&quot;&amp;StandF_d=&quot;&amp; Standard_UpFile &amp;&quot;&amp;Sit_d=&quot;&amp; ADisInSites &amp;&quot;&amp;Dis_d=&quot;&amp; CokeShow.CodeToURL(Summary,&quot;1@1&quot;) %&gt;&quot; target=&quot;_blank&quot; title=&quot;&lt;% =rsArea(&quot;AdPlace_ShowText&quot;) %&gt;&quot;&gt;&lt;img src=&quot;&lt;% If Len(Trim(rsArea(&quot;AdPlace_ShowPic&quot;))) = 0 Then %&gt;http://www.8588.cn/images_new/ad_233x100.gif&lt;% Else %&gt;http://www.mrpsdd.com&lt;% =rsArea(&quot;AdPlace_ShowPic&quot;) %&gt;&lt;% End If %&gt;&quot; width=&quot;&lt;% =picWidth %&gt;&quot; height=&quot;&lt;% =picHeight %&gt;&quot; alt=&quot;&lt;% =rsArea(&quot;AdPlace_ShowText&quot;) %&gt;&quot;  /&gt;&lt;/a&gt;&lt;%<BR>
			ElseIf isUseTemplate=1 Then<BR>
			'处理特殊模板1~<BR>
			%&gt;&lt;a href=&quot;&lt;% =rsArea(&quot;AdPlace_LinkUrl&quot;) %&gt;&lt;% If Instr(rsArea(&quot;AdPlace_LinkUrl&quot;),&quot;?&quot;)&gt;0 Then Response.Write &quot;&amp;&quot; Else Response.Write &quot;?&quot; %&gt;&lt;% =&quot;isP_d=&quot;&amp; isPic &amp;&quot;&amp;pW_d=&quot;&amp; picWidth &amp;&quot;&amp;pH_d=&quot;&amp; picHeight &amp;&quot;&amp;StandW_d=&quot;&amp; Standard_UpWord &amp;&quot;&amp;isUT_d=&quot;&amp; isUseTemplate &amp;&quot;&amp;StandF_d=&quot;&amp; Standard_UpFile &amp;&quot;&amp;Sit_d=&quot;&amp; ADisInSites &amp;&quot;&amp;Dis_d=&quot;&amp; CokeShow.CodeToURL(Summary,&quot;1@1&quot;) %&gt;&quot; target=&quot;_blank&quot; title=&quot;&lt;% =rsArea(&quot;AdPlace_ShowText&quot;) %&gt;&quot;&gt;&lt;img src=&quot;&lt;% If Len(Trim(rsArea(&quot;AdPlace_ShowPic&quot;))) = 0 Then %&gt;http://www.8588.cn/images_new/ad_233x100.gif&lt;% Else %&gt;http://www.mrpsdd.com&lt;% =rsArea(&quot;AdPlace_ShowPic&quot;) %&gt;&lt;% End If %&gt;&quot; width=&quot;&lt;% =picWidth %&gt;&quot; height=&quot;&lt;% =picHeight %&gt;&quot; alt=&quot;&lt;% =rsArea(&quot;AdPlace_ShowText&quot;) %&gt;&quot;  /&gt;&lt;span style=&quot;cursor:hand;&quot;&gt;&lt;% =rsArea(&quot;AdPlace_ShowText&quot;) %&gt;&lt;/span&gt;&lt;/a&gt;&lt;%<BR>
			ElseIf isUseTemplate=2 Then<BR>
			'处理特殊模板2~<BR>
			%&gt;&lt;a href=&quot;&lt;% =rsArea(&quot;AdPlace_LinkUrl&quot;) %&gt;&lt;% If Instr(rsArea(&quot;AdPlace_LinkUrl&quot;),&quot;?&quot;)&gt;0 Then Response.Write &quot;&amp;&quot; Else Response.Write &quot;?&quot; %&gt;&lt;% =&quot;isP_d=&quot;&amp; isPic &amp;&quot;&amp;pW_d=&quot;&amp; picWidth &amp;&quot;&amp;pH_d=&quot;&amp; picHeight &amp;&quot;&amp;StandW_d=&quot;&amp; Standard_UpWord &amp;&quot;&amp;isUT_d=&quot;&amp; isUseTemplate &amp;&quot;&amp;StandF_d=&quot;&amp; Standard_UpFile &amp;&quot;&amp;Sit_d=&quot;&amp; ADisInSites &amp;&quot;&amp;Dis_d=&quot;&amp; CokeShow.CodeToURL(Summary,&quot;1@1&quot;) %&gt;&quot; target=&quot;_blank&quot; title=&quot;&lt;% =rsArea(&quot;AdPlace_ShowText&quot;) %&gt;&quot;&gt;&lt;img src=&quot;&lt;% If Len(Trim(rsArea(&quot;AdPlace_ShowPic&quot;))) = 0 Then %&gt;http://www.8588.cn/images_new/ad_233x100.gif&lt;% Else %&gt;http://www.mrpsdd.com&lt;% =rsArea(&quot;AdPlace_ShowPic&quot;) %&gt;&lt;% End If %&gt;&quot; width=&quot;&lt;% =picWidth %&gt;&quot; height=&quot;&lt;% =picHeight %&gt;&quot; alt=&quot;&lt;% =rsArea(&quot;AdPlace_ShowText&quot;) %&gt;&quot;  /&gt;&lt;EM style=&quot;cursor:hand;&quot;&gt;&lt;% =rsArea(&quot;AdPlace_ShowText&quot;) %&gt;&lt;/EM&gt;&lt;/a&gt;&lt;%<BR>
			ElseIf isUseTemplate=3 Then<BR>
			'处理特殊模板3~<BR>
			%&gt;&lt;a href=&quot;&lt;% =rsArea(&quot;AdPlace_LinkUrl&quot;) %&gt;&lt;% If Instr(rsArea(&quot;AdPlace_LinkUrl&quot;),&quot;?&quot;)&gt;0 Then Response.Write &quot;&amp;&quot; Else Response.Write &quot;?&quot; %&gt;&lt;% =&quot;isP_d=&quot;&amp; isPic &amp;&quot;&amp;pW_d=&quot;&amp; picWidth &amp;&quot;&amp;pH_d=&quot;&amp; picHeight &amp;&quot;&amp;StandW_d=&quot;&amp; Standard_UpWord &amp;&quot;&amp;isUT_d=&quot;&amp; isUseTemplate &amp;&quot;&amp;StandF_d=&quot;&amp; Standard_UpFile &amp;&quot;&amp;Sit_d=&quot;&amp; ADisInSites &amp;&quot;&amp;Dis_d=&quot;&amp; CokeShow.CodeToURL(Summary,&quot;1@1&quot;) %&gt;&quot; target=&quot;_blank&quot; title=&quot;&lt;% =rsArea(&quot;AdPlace_ShowText&quot;) %&gt;&quot;&gt;&lt;img src=&quot;&lt;% If Len(Trim(rsArea(&quot;AdPlace_ShowPic&quot;))) = 0 Then %&gt;http://www.8588.cn/images_new/ad_233x100.gif&lt;% Else %&gt;http://www.mrpsdd.com&lt;% =rsArea(&quot;AdPlace_ShowPic&quot;) %&gt;&lt;% End If %&gt;&quot; width=&quot;&lt;% =picWidth %&gt;&quot; height=&quot;&lt;% =picHeight %&gt;&quot; alt=&quot;&lt;% =rsArea(&quot;AdPlace_ShowText&quot;) %&gt;&quot;  /&gt;&lt;p style=&quot;cursor:hand;&quot;&gt;&lt;% =rsArea(&quot;AdPlace_ShowText&quot;) %&gt;&lt;/p&gt;&lt;/a&gt;&lt;%<BR>
			End If<BR>
		<BR>
		<BR>
		<BR>
		Else<BR>
		<BR>
			%&gt;&lt;%<BR>
			Dim RSAuto_ADLogicAreaName<BR>
			OpenDB<BR>
			Set RSAuto_ADLogicAreaName = Conn.Execute( &quot;SELECT * FROM [system_AdDistrict] WHERE AdDistrict_Name = '&quot;&amp; Left( Summary, Len(Summary)-2 ) &amp;&quot;'&quot; )<BR>
			<BR>
			If Not RSAuto_ADLogicAreaName.Eof Then<BR>
				'$B1$<BR>
			%&gt;&lt;% '注意：UseTemplateHTML标签字符如果长度太长，就会报错！（令一些图片$标签根本无法还原成图片） %&gt;$8588&lt;% =RSAuto_ADLogicAreaName(&quot;AdDistrict_Auto_AreaName&quot;) %&gt;(isP==1,aLC==&lt;% =aLinkClassname %&gt;,aLSC==&lt;% =aLinkStyleColor %&gt;,pW==&lt;% =picWidth %&gt;,pH==&lt;% =picHeight %&gt;,S_UW==&lt;% =Standard_UpWord %&gt;,isUT==&lt;% =isUseTemplate %&gt;,InSites==&lt;% =ADisInSites %&gt;,S_UF==&lt;% =Standard_UpFile %&gt;,iRN==&lt;% =&quot;^^^&quot;&amp; Summary %&gt;&lt;% =&quot;|chris|&quot;&amp; intRndNumber %&gt;)$&lt;%<BR>
			<BR>
			Else<BR>
			<BR>
			%&gt;&lt;% '=SQLAuto_ADLogicAreaName %&gt;&lt;%<BR>
			<BR>
			End If<BR>
			<BR>
			%&gt;&lt;%<BR>
			<BR>
		End If<BR>
		<BR>
		<BR>
		<BR>
		<BR>
		rsArea.Close<BR>
		Set rsArea = Nothing<BR>
		<BR>
	End Sub<BR>
	<BR>
	<BR>
	'--------------------------------------<BR>
	'///            文字链,通用			///<BR>
	'/// width：无   				///<BR>
	'/// height：无   			///<BR>
	'/// alt：无    ///<BR>
	'/// Standard_UpWord：使用4、6、8、10、12、更多 类型的广告词词库文字？！0代表随便任何一种类型皆可！(自动化时用)<BR>
	'--------------------------------------<BR>
	Public Sub areaNoPic(Summary,isPic,aLinkClassname,aLinkStyleColor,picWidth,picHeight,Standard_UpWord,isUseTemplate,ADisInSites,Standard_UpFile,paraintRndNumber)<BR>
		Randomize<BR>
		Dim intRndNumber<BR>
		intRndNumber = Int(10000*Rnd) + 1 &amp; Second(Time()) &amp; CokeShow.GetRandomizeCode()<BR>
		If paraintRndNumber&lt;&gt;&quot;&quot; Then intRndNumber=paraintRndNumber<BR>
		<BR>
		Dim rsArea<BR>
		<BR>
		OpenDb<BR>
		Set rsArea = Server.CreateObject(&quot;ADODB.RecordSet&quot;)<BR>
		'rsArea.Open &quot;Select Top 1 AdPlace_ShowTitle,AdPlace_ShowPic,AdPlace_LinkUrl,AdPlace_ShowText From [system_AdPlace] Where AdPlace_Summary = '&quot;&amp; Summary &amp;&quot;' And AdPlace_Enable = 1 AND ('&quot;&amp; date() &amp;&quot;' BETWEEN AdPlace_StartDate AND AdPlace_EndDate)&quot;,conn,1,1<BR>
			<BR>
		rsArea.Open &quot;SELECT TOP 1 &quot;&amp;_<BR>
			&quot;AdPlace_ShowTitle,AdPlace_ShowPic,AdPlace_LinkUrl,AdPlace_ShowText &quot;&amp;_<BR>
		&quot;FROM [system_AdPlace] ADP &quot;&amp;_<BR>
			&quot;LEFT JOIN [system_AdList] AD ON (ADP.AdPlace_AdListID = AD.AdList_ID AND AD.AdList_sysLogic_Deleted=0 ) &quot;&amp;_<BR>
		&quot;WHERE 1=1 AND AdPlace_sysLogic_Deleted=0 &quot;&amp;_<BR>
			&quot;AND AdPlace_Summary = '&quot;&amp; Summary &amp;&quot;' &quot;&amp;_<BR>
			&quot;AND AdPlace_Enable = 1 &quot;&amp;_<BR>
			&quot;/*AND AdPlace_IsMain = 1*/ &quot;&amp;_<BR>
			&quot;AND AdList_ID IS NOT NULL &quot;&amp;_<BR>
			&quot;AND AdList_EnGuestBook=1 &quot;&amp;_<BR>
			&quot;AND DATEDIFF(day,AdList_StartDate,GETDATE())&gt;=0 &quot;,Conn,1,1<BR>
			'【手动化】<BR>
			'01条件：已启用的位，可以Show出来！否则也自动下线！！！~~~<BR>
			'01条件：顺便过滤掉那些没有关联ADID的广告位（孤儿）！~<BR>
			'02条件：到期日的下午18点时，自动下线整个广告！！！~~~<BR>
			'02条件：DATEDIFF(hh, CONVERT( varchar(10), '2013-07-03 11:11:11', 120 ), GETDATE()),* 【CONVERT函数，确保尾部时间不进行计算！即被当作00:00:00来计算。】<BR>
			'03条件：开始日期当天（立刻），开始生效，并Show出具体广告位来！~【从起始日期的0点开始生效】<BR>
			'&amp;_<BR>
			'&quot;AND (   (DATEDIFF(hh, CONVERT( varchar(10), AdList_EndDate, 120 ), GETDATE())&lt;18 AND AdList_isForTimesOrBooks=0)   OR   ( AdList_ADNeedMessageNum_InAllDates&gt;(SELECT COUNT(book_id) FROM [system_Count_Book] WHERE 1=1 AND Book_sysLogic_Deleted=0 AND Book_isDiscontinue=0 AND DATEDIFF(d, AdList_StartDate, Book_AddTime)&gt;=0 AND book_AdListid=AD.AdList_ID) AND AdList_isForTimesOrBooks=1)   ) &quot;<BR>
		<BR>
		If Not rsArea.Eof Then<BR>
			<BR>
			If isUseTemplate=0 Then<BR>
			'只处理a、img的这部分（通用，随处可用！）- 若有其它标签则用HTML模板来处理！~<BR>
			%&gt;&lt;a href=&quot;&lt;% =rsArea(&quot;AdPlace_LinkUrl&quot;) %&gt;&lt;% If Instr(rsArea(&quot;AdPlace_LinkUrl&quot;),&quot;?&quot;)&gt;0 Then Response.Write &quot;&amp;&quot; Else Response.Write &quot;?&quot; %&gt;&lt;% =&quot;isP_d=&quot;&amp; isPic &amp;&quot;&amp;pW_d=&quot;&amp; picWidth &amp;&quot;&amp;pH_d=&quot;&amp; picHeight &amp;&quot;&amp;StandW_d=&quot;&amp; Standard_UpWord &amp;&quot;&amp;isUT_d=&quot;&amp; isUseTemplate &amp;&quot;&amp;StandF_d=&quot;&amp; Standard_UpFile &amp;&quot;&amp;Sit_d=&quot;&amp; ADisInSites &amp;&quot;&amp;Dis_d=&quot;&amp; CokeShow.CodeToURL(Summary,&quot;1@1&quot;) %&gt;&quot; target=&quot;_blank&quot; title=&quot;&lt;% =rsArea(&quot;AdPlace_ShowText&quot;) %&gt;&quot; &lt;% If Len(aLinkClassname)&gt;0 Then Response.Write &quot;class=&quot;&quot;&quot;&amp; aLinkClassname &amp;&quot;&quot;&quot;&quot; %&gt; &lt;% If Len(aLinkStyleColor)&gt;0 Then Response.Write &quot;style=&quot;&quot;color:&quot;&amp; aLinkStyleColor &amp;&quot;;&quot;&quot;&quot; %&gt;&gt;&lt;% =rsArea(&quot;AdPlace_ShowText&quot;) %&gt;&lt;/a&gt;&lt;%<BR>
			ElseIf isUseTemplate=1 Then<BR>
			'处理特殊模板1~<BR>
			%&gt;&lt;a href=&quot;&lt;% =rsArea(&quot;AdPlace_LinkUrl&quot;) %&gt;&lt;% If Instr(rsArea(&quot;AdPlace_LinkUrl&quot;),&quot;?&quot;)&gt;0 Then Response.Write &quot;&amp;&quot; Else Response.Write &quot;?&quot; %&gt;&lt;% =&quot;isP_d=&quot;&amp; isPic &amp;&quot;&amp;pW_d=&quot;&amp; picWidth &amp;&quot;&amp;pH_d=&quot;&amp; picHeight &amp;&quot;&amp;StandW_d=&quot;&amp; Standard_UpWord &amp;&quot;&amp;isUT_d=&quot;&amp; isUseTemplate &amp;&quot;&amp;StandF_d=&quot;&amp; Standard_UpFile &amp;&quot;&amp;Sit_d=&quot;&amp; ADisInSites &amp;&quot;&amp;Dis_d=&quot;&amp; CokeShow.CodeToURL(Summary,&quot;1@1&quot;) %&gt;&quot; target=&quot;_blank&quot; title=&quot;&lt;% =rsArea(&quot;AdPlace_ShowText&quot;) %&gt;&quot; &lt;% If Len(aLinkClassname)&gt;0 Then Response.Write &quot;class=&quot;&quot;&quot;&amp; aLinkClassname &amp;&quot;&quot;&quot;&quot; %&gt; &lt;% If Len(aLinkStyleColor)&gt;0 Then Response.Write &quot;style=&quot;&quot;color:&quot;&amp; aLinkStyleColor &amp;&quot;;&quot;&quot;&quot; %&gt;&gt;&lt;% =rsArea(&quot;AdPlace_ShowText&quot;) %&gt;&lt;span style=&quot;cursor:hand;&quot;&gt;&lt;% =rsArea(&quot;AdPlace_ShowText&quot;) %&gt;&lt;/span&gt;&lt;/a&gt;&lt;%<BR>
			End If<BR>
		<BR>
		<BR>
		<BR>
		Else<BR>
		<BR>
			%&gt;&lt;%<BR>
			Dim RSAuto_ADLogicAreaName<BR>
			OpenDB<BR>
			Set RSAuto_ADLogicAreaName = Conn.Execute( &quot;SELECT * FROM [system_AdDistrict] WHERE AdDistrict_Name = '&quot;&amp; getSring_NoLastStringA( Left( Summary, Len(Summary)-1 ), &quot;-&quot; ) &amp;&quot;'&quot; )<BR>
			<BR>
			If Not RSAuto_ADLogicAreaName.Eof Then<BR>
				'$B1$<BR>
			%&gt;&lt;% '注意：UseTemplateHTML标签字符如果长度太长，就会报错！（令一些图片$标签根本无法还原成图片） %&gt;<BR>
				$WL8588&lt;% =RSAuto_ADLogicAreaName(&quot;AdDistrict_Auto_AreaName&quot;) %&gt;(isP==0,aLC==&lt;% =aLinkClassname %&gt;,aLSC==&lt;% =aLinkStyleColor %&gt;,pW==&lt;% =picWidth %&gt;,pH==&lt;% =picHeight %&gt;,S_UW==&lt;% =Standard_UpWord %&gt;,isUT==&lt;% =isUseTemplate %&gt;,InSites==&lt;% =ADisInSites %&gt;,S_UF==&lt;% =Standard_UpFile %&gt;,iRN==&lt;% =&quot;^^^&quot;&amp; Summary %&gt;&lt;% '=intRndNumber %&gt;)$&lt;%<BR>
			<BR>
			Else<BR>
			<BR>
			%&gt;&lt;% '=getSring_NoLastStringA( Left( Summary, Len(Summary)-1 ), &quot;-&quot; ) %&gt;&lt;%<BR>
			<BR>
			End If<BR>
			<BR>
			%&gt;&lt;%<BR>
			<BR>
		End If<BR>
		<BR>
		<BR>
		<BR>
		<BR>
		rsArea.Close<BR>
		Set rsArea = Nothing<BR>
		<BR>
	End Sub<BR>
	<BR>
	<BR>
	'函数（根据当前时间，决定（自动）分配留言的百分比%）<BR>
	'例如：<BR>
	'	上午(7~12点钟的时候)留言自动分配50%为止，则输出0.50；<BR>
	'	下午(13~20点钟的时候)80%；<BR>
	'	晚上(21~23点钟的时候)随意到100%；<BR>
	'	0~6点钟的时候，不许留言分配进来！~~~（设为1%即可！）[BETWEEN 0 AND 0.01-几乎没有留言进来的可能！]<BR>
	Public Function getBooksPercentNum_ForDiffTimes()<BR>
		Dim strShow,funcFoundErr<BR>
		funcFoundErr= False<BR>
		strShow = &quot;&quot;<BR>
		<BR>
		'校验数据：<BR>
		If 1=2 Then<BR>
			funcFoundErr = True<BR>
			ErrMsg = ErrMsg &amp;&quot;the result is wrong!~~~&quot;<BR>
			Exit Function<BR>
		End If<BR>
		OpenDb<BR>
		<BR>
		'验证<BR>
'Response.Write &quot;wwwwwwwww&quot;<BR>
		<BR>
		'处理并计算数据：<BR>
		'strShow = &quot;撒酒疯离开家阿DFJLK;AJFKLJ斯利康房价&quot;<BR>
					<BR>
		'拦截错误，不然错误往下进行！<BR>
		If funcFoundErr=True Then<BR>
			Response.Write &quot;&lt;script&gt;alert(&quot;&quot;&quot;&amp; ErrMsg &amp;&quot;&quot;&quot;); history.go(-1);&lt;/script&gt;&quot;<BR>
			Response.End()<BR>
			'Exit Sub<BR>
		End If<BR>
		'Chris End<BR>
		<BR>
		'返回百分比数据：<BR>
		Dim intNowHour<BR>
		intNowHour = CokeShow.CokeClng( Hour(Now()) )<BR>
		<BR>
		'凌晨问候.<BR>
		If intNowHour&gt;=0 And intNowHour&lt;7 Then<BR>
			'SayHello=&quot;凌晨好&quot;<BR>
			strShow = &quot; 0.003 &quot;	'完全 没有留言进来的可能！！！！！！！！！！！！！~<BR>
		End If<BR>
		<BR>
		'早上问候.<BR>
		If intNowHour&gt;=7 And intNowHour&lt;13 Then<BR>
			'SayHello=&quot;早上好&quot;<BR>
			strShow = &quot; 0.50 &quot;<BR>
		End If<BR>
		<BR>
		'下午问候.<BR>
		If intNowHour&gt;=13 And intNowHour&lt;18 Then<BR>
			'SayHello=&quot;下午好&quot;<BR>
			strShow = &quot; 0.80 &quot;<BR>
		End If<BR>
		<BR>
		'晚上问候.<BR>
		If intNowHour&gt;=18 And intNowHour&lt;24 Then<BR>
			'SayHello=&quot;晚上好&quot;<BR>
			strShow = &quot; 0.99 &quot;<BR>
		End If<BR>
		<BR>
		'strShow = True<BR>
		'返回结果：<BR>
		getBooksPercentNum_ForDiffTimes = strShow<BR>
	End Function<BR>
	<BR>
	<BR>
	'函数（计算48小时后，返回为True）<BR>
	'例如：<BR>
	'	默认为False.<BR>
	'例子：<BR>
	'	If CokeShow.isAlertDayNow(xxx)=True Then...<BR>
	Public Function isAlertDayNow(strDateTime)<BR>
		Dim strShow,funcFoundErr<BR>
		funcFoundErr= False<BR>
		strShow = &quot;&quot;<BR>
		isAlertDayNow = False<BR>
		<BR>
		'校验数据：<BR>
		If 1=2 Then<BR>
			funcFoundErr = True<BR>
			ErrMsg = ErrMsg &amp;&quot;the result is wrong!~~~&quot;<BR>
			Exit Function<BR>
		End If<BR>
		<BR>
		'验证<BR>
		If isDate(strDateTime) Then<BR>
			funcFoundErr = True<BR>
			ErrMsg = ErrMsg &amp;&quot;时间格式不对.&quot;<BR>
			Exit Function<BR>
		End If<BR>
		<BR>
		'处理并计算数据：<BR>
		<BR>
		'拦截错误，不然错误往下进行！<BR>
		If funcFoundErr=True Then<BR>
			Response.Write &quot;&lt;script&gt;alert(&quot;&quot;&quot;&amp; ErrMsg &amp;&quot;&quot;&quot;); history.go(-1);&lt;/script&gt;&quot;<BR>
			Response.End()<BR>
			'Exit Sub<BR>
		End If<BR>
		'Chris End<BR>
		<BR>
		'返回True/False：<BR>
		If DateDiff(&quot;h&quot;, strDateTime, Cstr(Now()))&gt;=48 Then		'默认为2天后报警！！！~<BR>
			isAlertDayNow = True<BR>
			Exit Function<BR>
		Else<BR>
			'isAlertDayNow = False<BR>
		End If<BR>
		<BR>
		'strShow = True<BR>
		'返回结果：<BR>
		'isAlertDayNow = strShow<BR>
	End Function<BR>
	<BR>
End Class<BR>
%&gt;
</DD>

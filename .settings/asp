<DT>Sub_处理修改函数</DT>
<!--property:icon_internal=blank-->
<!--property:syntax_highlight=vbnet-->
<!--property:date_created=1364549833-->
<!--property:date_modified=1386919261-->
<DD>
Sub SaveModify()<BR>
	Dim intID<BR>
	Dim newID,newNAME<BR>
	Dim oneFormItem<BR>
	'获取Form表单参数：<BR>
	For Each oneFormItem In Request.Form<BR>
		Execute(&quot;Dim &quot;&amp; oneFormItem)<BR>
		Execute(&quot;&quot;&amp; oneFormItem &amp;&quot; = CokeShow.filtPublicRequest(Request.Form(&quot;&quot;&quot;&amp; oneFormItem &amp;&quot;&quot;&quot;))&quot;)<BR>
	Next<BR>
	'测试：<BR>
		'response.Write myname &amp;&quot;&lt;br /&gt;&quot;<BR>
		'response.Write mytel &amp;&quot;&lt;br /&gt;&quot;<BR>
		'response.Write remark &amp;&quot;&lt;br /&gt;&quot;<BR>
		'response.Write Action &amp;&quot;&lt;br /&gt;&quot;<BR>
		'Response.End()<BR>
	<BR>
	intID = id		'检测 id 参数.<BR>
	If intID=&quot;&quot; Then<BR>
		FoundErr = True<BR>
		ErrMsg = ErrMsg &amp;&quot;参数不足！&quot;<BR>
		Exit Sub<BR>
	Else<BR>
		intID = CokeShow.CokeClng(intID)<BR>
	End If<BR>
	<BR>
	'验证：<BR>
	'字符类:<BR>
	If myname=&quot;&quot; Then<BR>
		FoundErr = True<BR>
		ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;xxx&quot; &amp;&quot;不能为空！&quot;<BR>
	Else<BR>
		If CokeShow.strLength(myname)&gt;20 Or CokeShow.strLength(myname)&lt;2 Then<BR>
			FoundErr = True<BR>
			ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;xxx&quot; &amp;&quot;长度不能大于20个字符，也不能小于2个字符！&quot;<BR>
		Else<BR>
			myname = myname<BR>
		End If<BR>
	End If<BR>
	<BR>
	'数字类:<BR>
	If mytel=&quot;&quot; Then<BR>
		'FoundErr = True<BR>
		'ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;xxx&quot; &amp;&quot;不能为空！&quot;<BR>
	Else<BR>
		If isNumeric(mytel) Then<BR>
			mytel = CokeShow.CokeClng(mytel)<BR>
		Else<BR>
			FoundErr = True<BR>
			ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;xxx&quot; &amp;&quot;请用数字填写！&quot;<BR>
		End If<BR>
	End If<BR>
	'其它类型验证:<BR>
	'拦截错误！<BR>
	If FoundErr=True Then Exit Sub<BR>
'	Dim StrSql,iduType,iduName,iduFields,iduWhere<BR>
'	iduType=&quot;3&quot; : iduName=CurrentTableName : iduFields=&quot;&quot;		'==========begin===========<BR>
'	iduFields = iduFields &amp; &quot;id{=}&quot; 	&amp; id 		&amp; &quot;{,}&quot;<BR>
'	iduFields = iduFields &amp; &quot;myname{=}&quot; &amp; myname 	&amp; &quot;{,}&quot;<BR>
'	iduFields = iduFields &amp; &quot;mytel{=}&quot; 	&amp; mytel 	&amp; &quot;{,}&quot;<BR>
'	iduFields = iduFields &amp; &quot;remark{=}&quot; &amp; remark 	&amp; &quot;{,}&quot;<BR>
'	iduWhere = &quot;&quot; <BR>
'	StrSql = StrSql &amp; &quot;{|}&quot; &amp; IDU(Cstr(iduType), Cstr(iduName), Cstr(iduFields), Cstr(iduWhere))'======end=======<BR>
'	'response.Write postSQL(Cstr(StrSql))	'回滚用法<BR>
'	StrSql = postSQL(Cstr(StrSql))	'回滚用法<BR>
'	If StrSql=&quot;success&quot; Then<BR>
'		response.Write &quot;success&quot;<BR>
'		response.End()<BR>
'	Else<BR>
'		response.Write StrSql<BR>
'		response.End()	<BR>
'	End If<BR>
	<BR>
	Set RS = Server.CreateObject(&quot;Adodb.RecordSet&quot;)<BR>
	sql = &quot;SELECT * FROM &quot;&amp; CurrentTableName &amp;&quot; WHERE 0=0 AND isDeleted=0 AND id=&quot;&amp; intID<BR>
	'RS.Open sql,CONN,1,3<BR>
	RS.Open sql,ConnStr,adOpenKeyset,adLockOptimistic,adCmdText<BR>
	<BR>
	'拦截此记录的异常情况.<BR>
	If RS.Bof And RS.Eof Then<BR>
		FoundErr = True<BR>
		ErrMsg = ErrMsg &amp; &quot;找不到指定的&quot;&amp; UnitName &amp;&quot;！&quot;<BR>
		Exit Sub<BR>
	End If<BR>
		RS(&quot;myname&quot;)= myname<BR>
		RS(&quot;mytel&quot;)	= mytel<BR>
		RS(&quot;remark&quot;)= remark<BR>
	RS.Update<BR>
	'获取新增的ID：<BR>
	newID	= RS(&quot;user_userID&quot;)<BR>
	newNAME	= RS(&quot;user_userName&quot;)<BR>
	RS.Close<BR>
	Set RS = Nothing<BR>
	<BR>
	'记入日志.<BR>
	Call CokeShow.AddLog(0, CurrentTableName, newID, &quot;大后台编辑操作：成功编辑了ID为&quot;&amp; newID &amp;&quot;(&quot;&amp; newNAME &amp;&quot;)的&quot;&amp; UnitName &amp;&quot;！&quot;)<BR>
	Response.Write &quot;success&quot;<BR>
End Sub
</DD>
<DT>Sub_处理删除函数</DT>
<!--property:icon_internal=blank-->
<!--property:syntax_highlight=vbnet-->
<!--property:date_created=1364549833-->
<!--property:date_modified=1386919294-->
<DD>
Sub Delete()<BR>
	Dim intID<BR>
	Dim newID,newNAME,i_DeleteSub<BR>
	Dim oneFormItem<BR>
	'获取Form表单参数：<BR>
	For Each oneFormItem In Request.Form<BR>
		Execute(&quot;Dim &quot;&amp; oneFormItem)<BR>
		Execute(&quot;&quot;&amp; oneFormItem &amp;&quot; = CokeShow.filtPublicRequest(Request.Form(&quot;&quot;&quot;&amp; oneFormItem &amp;&quot;&quot;&quot;))&quot;)<BR>
	Next<BR>
	'测试：<BR>
		'response.Write myname &amp;&quot;&lt;br /&gt;&quot;<BR>
		'response.Write mytel &amp;&quot;&lt;br /&gt;&quot;<BR>
		'response.Write remark &amp;&quot;&lt;br /&gt;&quot;<BR>
		'response.Write Action &amp;&quot;&lt;br /&gt;&quot;<BR>
		'Response.End()<BR>
	<BR>
	intID = id		'检测 id 参数.<BR>
	If intID=&quot;&quot; Then<BR>
		FoundErr = True<BR>
		ErrMsg = ErrMsg &amp; &quot;请指定要删除的&quot;&amp; UnitName &amp;&quot;&quot;<BR>
	End If<BR>
	If isNumeric(intID) Then<BR>
		If intID=0 Then<BR>
			FoundErr = True<BR>
			ErrMsg = ErrMsg &amp; &quot;请指定要删除的&quot;&amp; UnitName &amp;&quot;&quot;<BR>
		End If<BR>
	End If<BR>
	<BR>
	'拦截错误！<BR>
	If FoundErr=True Then Exit Sub<BR>
	<BR>
	If Instr(intID,&quot;,&quot;)&gt;0 Then<BR>
		'intID = Split(intID,&quot;,&quot;)<BR>
		For i_DeleteSub=0 To Ubound( Split(intID,&quot;,&quot;) )<BR>
			DeleteOne( Split(intID,&quot;,&quot;)(i_DeleteSub) )<BR>
		Next<BR>
	ElseIf isNumeric(intID) Then<BR>
		DeleteOne(intID)<BR>
	Else<BR>
		FoundErr = True<BR>
		ErrMsg = ErrMsg &amp; &quot;请指定要删除的&quot;&amp; UnitName &amp;&quot;&quot;<BR>
		Exit Sub<BR>
	End If<BR>
	<BR>
	'记入日志.<BR>
	Call CokeShow.AddLog(0, CurrentTableName, intID, &quot;大后台删除操作：成功完成了批量删除ID为&quot;&amp; intID &amp;&quot;的&quot;&amp; UnitName &amp;&quot;！&quot;)<BR>
	Response.Write &quot;success&quot;<BR>
End Sub<BR>
<BR>
Sub DeleteOne(intID)<BR>
	Dim RS_Del<BR>
	intID = CokeShow.CokeClng(intID)<BR>
	If intID=0 Then Exit Sub'不理会0的ID的记录；（0一般仅仅是用作填充的）<BR>
	<BR>
	Set RS = Server.CreateObject(&quot;ADODB.RecordSet&quot;)<BR>
	sql = &quot;SELECT * FROM &quot;&amp; CurrentTableName &amp;&quot; WHERE 0=0 AND isDeleted=0 AND id=&quot;&amp; intID<BR>
	'RS.Open sql,Conn,3,1<BR>
	RS.Open sql,ConnStr,adOpenForwardOnly,adLockReadOnly,adCmdText<BR>
	If Not RS.Eof Then<BR>
		'//CONN.Execute(&quot;DELETE FROM &quot;&amp; CurrentTableName &amp;&quot; WHERE id=&quot;&amp; intID)<BR>
		Set RS_Del = Server.CreateObject(&quot;ADODB.RecordSet&quot;)<BR>
		sql = &quot;UPDATE &quot;&amp; CurrentTableName &amp;&quot; SET isDeleted=1 WHERE 0=0 AND isDeleted=0 AND id=&quot;&amp; intID<BR>
		RS_Del.Open sql,ConnStr,adOpenStatic,adLockReadOnly,adCmdText<BR>
		'RS_Del.Close<BR>
		'记入日志.<BR>
		'Call CokeShow.AddLog(1, CurrentTableName, intID, &quot;删除操作：成功删除了ID为&quot;&amp; intID &amp;&quot;的&quot;&amp; UnitName &amp;&quot;！&quot;)<BR>
	Else<BR>
		'找不着记录，则<BR>
		'记入日志.<BR>
		'Call CokeShow.AddLog(1, CurrentTableName, intID, &quot;删除操作：删除ID为&quot;&amp; intID &amp;&quot;的&quot;&amp; UnitName &amp;&quot;失败！此记录有可能早已丢失！&quot;)<BR>
		FoundErr = True<BR>
		ErrMsg = ErrMsg &amp; &quot;记录ID为&quot;&amp; intID &amp;&quot;的&quot;&amp; UnitName &amp;&quot;删除操作没成功，此记录有可能早已丢失！&quot;<BR>
		Exit Sub<BR>
	End If<BR>
End Sub
</DD>
<DT>Sub_处理添加函数</DT>
<!--property:icon_internal=blank-->
<!--property:syntax_highlight=vbnet-->
<!--property:date_created=1364549833-->
<!--property:date_modified=1386919325-->
<DD>
Sub SaveAdd()<BR>
	Dim newID,newNAME<BR>
	Dim oneFormItem<BR>
	'获取Form表单参数：<BR>
	For Each oneFormItem In Request.Form<BR>
		Execute(&quot;Dim &quot;&amp; oneFormItem)<BR>
		Execute(&quot;&quot;&amp; oneFormItem &amp;&quot; = CokeShow.filtPublicRequest(Request.Form(&quot;&quot;&quot;&amp; oneFormItem &amp;&quot;&quot;&quot;))&quot;)<BR>
	Next<BR>
	'测试：<BR>
		'response.Write myname &amp;&quot;&lt;br /&gt;&quot;<BR>
		'response.Write mytel &amp;&quot;&lt;br /&gt;&quot;<BR>
		'response.Write remark &amp;&quot;&lt;br /&gt;&quot;<BR>
		'response.Write Action &amp;&quot;&lt;br /&gt;&quot;<BR>
		'Response.End()<BR>
	<BR>
	'验证：<BR>
	'字符类:<BR>
	If myname=&quot;&quot; Then<BR>
		FoundErr = True<BR>
		ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;xxx&quot; &amp;&quot;不能为空！&quot;<BR>
	Else<BR>
		If CokeShow.strLength(myname)&gt;20 Or CokeShow.strLength(myname)&lt;2 Then<BR>
			FoundErr = True<BR>
			ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;xxx&quot; &amp;&quot;长度不能大于20个字符，也不能小于2个字符！&quot;<BR>
		Else<BR>
			myname = myname<BR>
		End If<BR>
	End If<BR>
	<BR>
	'数字类:<BR>
	If mytel=&quot;&quot; Then<BR>
		'FoundErr = True<BR>
		'ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;xxx&quot; &amp;&quot;不能为空！&quot;<BR>
	Else<BR>
		If isNumeric(mytel) Then<BR>
			mytel = CokeShow.CokeClng(mytel)<BR>
		Else<BR>
			FoundErr = True<BR>
			ErrMsg = ErrMsg &amp;&quot;&quot;&amp; &quot;xxx&quot; &amp;&quot;请用数字填写！&quot;<BR>
		End If<BR>
	End If<BR>
	'其它类型验证:<BR>
	'拦截错误，不然错误往下进行！<BR>
	If FoundErr=True Then Exit Sub<BR>
	<BR>
	Set RS = Server.CreateObject(&quot;Adodb.RecordSet&quot;)<BR>
	sql = &quot;SELECT * FROM &quot;&amp; CurrentTableName &amp;&quot; WHERE 0=0 AND isDeleted=0&quot;<BR>
	'RS.Open sql,CONN,2,2<BR>
	RS.Open sql,ConnStr,adOpenDynamic,adLockPessimistic,adCmdText<BR>
	RS.AddNew<BR>
		RS(&quot;myname&quot;)= myname<BR>
		RS(&quot;mytel&quot;)	= mytel<BR>
		RS(&quot;remark&quot;)= remark<BR>
	RS.Update<BR>
	'获取新增的ID：<BR>
	RS.MoveLast<BR>
	newID	= RS(&quot;id&quot;)<BR>
	newNAME	= RS(&quot;myname&quot;)<BR>
	RS.Close<BR>
	Set RS=Nothing<BR>
	<BR>
	'记入日志.<BR>
	Call CokeShow.AddLog(0, CurrentTableName, newID, &quot;大后台添加操作：成功添加了ID为&quot;&amp; newID &amp;&quot;(&quot;&amp; newNAME &amp;&quot;)的&quot;&amp; UnitName &amp;&quot;！&quot;)<BR>
	Response.Write &quot;success&quot;<BR>
End Sub
</DD>
<DT>头部定义</DT>
<!--property:icon_internal=blank-->
<!--property:syntax_highlight=vbnet-->
<!--property:date_created=1364549833-->
<!--property:date_modified=1386919345-->
<DD>
&lt;!--#include file=&quot;../inc/conn.asp&quot; --&gt;<BR>
&lt;%<BR>
'变量定义区.<BR>
'(用来存储对象的变量，用全大写!)<BR>
Dim TitleName,UnitName<BR>
TitleName 	= &quot;记录集管理&quot;				'此模块管理页的名字.<BR>
UnitName 	= &quot;记录&quot;					'此模块涉及记录的元素名.<BR>
'自定义设置.<BR>
'本地设置.<BR>
Dim CurrentTableName<BR>
CurrentTableName = &quot;test_wang&quot;		'此模块涉及的[表]名.<BR>
%&gt;<BR>
&lt;%<BR>
Dim RS, sql				'查询列表记录用的变量.<BR>
Dim FoundErr,ErrMsg		'控制错误流程用的控制变量.<BR>
Dim Action				'构建查询字符串以及流程控制用的控制变量.<BR>
Action = CokeShow.filtPublicRequest(Request.QueryString(&quot;Action&quot;))<BR>
<BR>
If Action=&quot;SaveAdd&quot; Then<BR>
	Call SaveAdd()<BR>
ElseIf Action=&quot;SaveModify&quot; Then<BR>
	Call SaveModify()<BR>
ElseIf Action=&quot;Delete&quot; Then<BR>
	Call Delete()<BR>
ElseIf Action=&quot;Lock&quot; Then<BR>
	Call Lock()<BR>
ElseIf Action=&quot;UnLock&quot; Then<BR>
	Call UnLock()<BR>
Else<BR>
	'Call Main()<BR>
End If<BR>
<BR>
'拦截.<BR>
If FoundErr=True Then<BR>
	Response.Write ErrMsg<BR>
	Response.End()<BR>
End If
</DD>
</DL>

</DD>

			&lt;input type=&quot;text&quot; name=&quot;user_userEmail&quot; id=&quot;user_userEmail&quot; value=&quot;&lt;%= RS(&quot;user_userEmail&quot;) %&gt;&quot; /&gt;<BR>
        &lt;/td&gt;<BR>
      &lt;/tr&gt;<BR>

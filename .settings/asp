<DT>表 user_user</DT>
<!--property:icon_internal=blank-->
<!--property:syntax_highlight=vbnet-->
<!--property:date_created=1364549833-->
<!--property:date_modified=1386924103-->
<DD>
CREATE TABLE [eBusiness].[user_user](<BR>
	[user_userID] [int] IDENTITY(1,1) NOT NULL,<BR>
	[user_userUsername] [nvarchar](30) COLLATE Chinese_PRC_CI_AS NOT NULL CONSTRAINT [DF_user_user_user_userUsername]  DEFAULT (''),<BR>
	[user_userPassword] [nvarchar](30) COLLATE Chinese_PRC_CI_AS NOT NULL CONSTRAINT [DF_user_user_user_userPassword]  DEFAULT (''),<BR>
	[user_groupID] [int] NOT NULL CONSTRAINT [DF_user_user_user_groupID]  DEFAULT ((0)),<BR>
	[user_userExpiredTime] [smalldatetime] NOT NULL CONSTRAINT [DF_user_user_user_userExpiredTime]  DEFAULT (getdate()),<BR>
	[user_userLastloginTime] [smalldatetime] NULL,<BR>
	[user_userLastloginIP] [nvarchar](15) COLLATE Chinese_PRC_CI_AS NOT NULL CONSTRAINT [DF_user_user_user_userLastloginIP]  DEFAULT (''),<BR>
	[user_userAddTime] [smalldatetime] NOT NULL CONSTRAINT [DF_user_user_user_userAddTime]  DEFAULT (getdate()),<BR>
	[user_userIsDeleted] [smallint] NOT NULL CONSTRAINT [DF_user_user_user_userIsDeleteded]  DEFAULT ((0)),<BR>
	[user_userName] [nvarchar](10) COLLATE Chinese_PRC_CI_AS NOT NULL CONSTRAINT [DF_user_user_user_userName]  DEFAULT (''),<BR>
	[user_userSex] [smallint] NOT NULL CONSTRAINT [DF_user_user_user_userSex]  DEFAULT ((0)),<BR>
	[user_userCookiesKey] [nvarchar](50) COLLATE Chinese_PRC_CI_AS NOT NULL CONSTRAINT [DF_user_user_user_userCookiesKey]  DEFAULT (''),<BR>
	[user_userRemark] [nvarchar](250) COLLATE Chinese_PRC_CI_AS NOT NULL CONSTRAINT [DF_user_user_user_userRemark]  DEFAULT (''),<BR>
	[user_userTel] [nvarchar](50) COLLATE Chinese_PRC_CI_AS NOT NULL CONSTRAINT [DF_user_user_user_userTel]  DEFAULT (''),<BR>
	[user_userEmail] [nvarchar](50) COLLATE Chinese_PRC_CI_AS NOT NULL CONSTRAINT [DF_user_user_user_userEmail]  DEFAULT (''),<BR>
	[user_userQQ] [nvarchar](12) COLLATE Chinese_PRC_CI_AS NOT NULL CONSTRAINT [DF_user_user_user_userQQ]  DEFAULT (''),<BR>
 CONSTRAINT [PK_user_user] PRIMARY KEY CLUSTERED <BR>
(<BR>
	[user_userID] ASC<BR>
)WITH (IGNORE_DUP_KEY = OFF) ON [PRIMARY]<BR>
) ON [PRIMARY]<BR>

</DD>
</DL>

</DD>
</DL>


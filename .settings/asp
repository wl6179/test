        &lt;td width=&quot;24%&quot;&gt;&lt;div align=&quot;right&quot;&gt;QQ号：&lt;/div&gt;&lt;/td&gt;<BR>
        &lt;td width=&quot;76%&quot; align=&quot;left&quot;&gt;<BR>
			&lt;input type=&quot;text&quot; name=&quot;user_userQQ&quot; id=&quot;user_userQQ&quot; value=&quot;&lt;%= RS(&quot;user_userQQ&quot;) %&gt;&quot; /&gt;<BR>
        &lt;/td&gt;<BR>
      &lt;/tr&gt;<BR>
      &lt;tr&gt;<BR>
        &lt;td width=&quot;24%&quot;&gt;&lt;div align=&quot;right&quot;&gt;备注：&lt;/div&gt;&lt;/td&gt;<BR>
        &lt;td width=&quot;76%&quot; align=&quot;left&quot;&gt;<BR>
        	&lt;textarea name=&quot;user_userRemark&quot; id=&quot;user_userRemark&quot; cols=&quot;45&quot; rows=&quot;5&quot;&gt;&lt;%= RS(&quot;user_userRemark&quot;) %&gt;&lt;/textarea&gt;<BR>
        &lt;/td&gt;<BR>
      &lt;/tr&gt;<BR>
      &lt;tr&gt;<BR>
        &lt;td width=&quot;24%&quot;&gt;&lt;div align=&quot;right&quot;&gt;创建时间：&lt;/div&gt;&lt;/td&gt;<BR>
        &lt;td width=&quot;76%&quot; align=&quot;left&quot;&gt;<BR>
        	&lt;%= RS(&quot;user_userAddTime&quot;) %&gt;<BR>
        &lt;/td&gt;<BR>
      &lt;/tr&gt;<BR>
      &lt;tr&gt;<BR>
        &lt;td width=&quot;24%&quot;&gt;&lt;div align=&quot;right&quot;&gt;最后登录时间：&lt;/div&gt;&lt;/td&gt;<BR>
        &lt;td width=&quot;76%&quot; align=&quot;left&quot;&gt;<BR>
        	&lt;%= RS(&quot;user_userLastloginTime&quot;) %&gt;<BR>
        &lt;/td&gt;<BR>
      &lt;/tr&gt;<BR>
      &lt;tr&gt;<BR>
        &lt;td width=&quot;24%&quot;&gt;&lt;div align=&quot;right&quot;&gt;最后登录IP：&lt;/div&gt;&lt;/td&gt;<BR>
        &lt;td width=&quot;76%&quot; align=&quot;left&quot;&gt;<BR>
        	&lt;%= RS(&quot;user_userLastloginIP&quot;) %&gt;<BR>
        &lt;/td&gt;<BR>
      &lt;/tr&gt;<BR>
      &lt;tr&gt;<BR>
        &lt;td width=&quot;24%&quot;&gt;&lt;div align=&quot;right&quot;&gt;密码到期日期：&lt;/div&gt;&lt;/td&gt;<BR>
        &lt;td width=&quot;76%&quot; align=&quot;left&quot;&gt;<BR>
        	&lt;%= RS(&quot;user_userExpiredTime&quot;) %&gt;<BR>
        &lt;/td&gt;<BR>
      &lt;/tr&gt;<BR>
      &lt;tr&gt;<BR>
        &lt;td colspan=&quot;2&quot;&gt;<BR>
       	&lt;div align=&quot;center&quot;&gt;<BR>
       	  &lt;input type=&quot;button&quot; name=&quot;modifybtn&quot; id=&quot;modifybtn&quot; value=&quot;修改记录&quot; style=&quot;padding:10px 20px;&quot; /&gt;<BR>
          <BR>
          &lt;input type=&quot;hidden&quot; name=&quot;id&quot; id=&quot;id&quot; value=&quot;&lt;% =RS(&quot;user_userID&quot;) %&gt;&quot; /&gt;<BR>
   	    &lt;/div&gt;&lt;/td&gt;<BR>
      &lt;/tr&gt;<BR>
    &lt;/table&gt;<BR>
    <BR>
    &lt;!--公共修改函数--&gt;<BR>
    &lt;script type=&quot;text/javascript&quot;&gt;<BR>
		$(document).ready(function(){<BR>
			$(&quot;#modifybtn&quot;).click(function(){<BR>
				if ($(&quot;#user_userUsername&quot;).val()==&quot;&quot; ){<BR>
					alert(&quot;&quot;+&quot;帐号不能为空！&quot;+&quot;&quot;);<BR>
					return;<BR>
				}<BR>
				if ($(&quot;#user_userName&quot;).val()==&quot;&quot; ){<BR>
					alert(&quot;&quot;+&quot;姓名不能为空！&quot;+&quot;&quot;);<BR>
					return;<BR>
				}<BR>
				var id					= escape($(&quot;#id&quot;).val());<BR>
				var user_userUsername	= escape($(&quot;#user_userUsername&quot;).val());<BR>
				var user_userPassword	= escape($(&quot;#user_userPassword&quot;).val());<BR>
				var user_userPassword_confirm	= escape($(&quot;#user_userPassword_confirm&quot;).val());<BR>
				var user_groupID		= escape($(&quot;#user_groupID&quot;).val());<BR>
				var user_userName		= escape($(&quot;#user_userName&quot;).val());<BR>
				var user_userSex		= escape($(&quot;#user_userSex&quot;).val());<BR>
				var user_userTel		= escape($(&quot;#user_userTel&quot;).val());<BR>
				var user_userEmail		= escape($(&quot;#user_userEmail&quot;).val());<BR>
				var user_userQQ			= escape($(&quot;#user_userQQ&quot;).val());<BR>
				var user_userRemark		= escape($(&quot;#user_userRemark&quot;).val());<BR>
				//$(&quot;#button1&quot;).hide();<BR>
				$(&quot;#modifybtn&quot;).toggle();	//提交时，隐藏提交按钮；<BR>
				$.ajax({url:&quot;master_user_user.Post.asp?Action=SaveModify&quot;,type:&quot;POST&quot;,data:&quot;&quot;<BR>
					+ &quot;Chris=&quot;	//操作.<BR>
					+ &quot;&amp;id=&quot;+ id<BR>
					+ &quot;&amp;user_userUsername=&quot;+ user_userUsername<BR>
					+ &quot;&amp;user_userPassword=&quot;+ user_userPassword<BR>
					+ &quot;&amp;user_userPassword_confirm=&quot;+ user_userPassword_confirm<BR>
					+ &quot;&amp;user_groupID=&quot;+ user_groupID<BR>
					+ &quot;&amp;user_userName=&quot;+ user_userName<BR>
					+ &quot;&amp;user_userSex=&quot;+ user_userSex<BR>
					+ &quot;&amp;user_userTel=&quot;+ user_userTel<BR>
					+ &quot;&amp;user_userEmail=&quot;+ user_userEmail<BR>
					+ &quot;&amp;user_userQQ=&quot;+ user_userQQ<BR>
					+ &quot;&amp;user_userRemark=&quot;+ user_userRemark<BR>
					+ &quot;&quot;,success: function(msg){<BR>
					if (msg==&quot;success&quot;){<BR>
						//$(&quot;#aaa&quot;).text(&quot;数据处理中。。。&quot;);<BR>
						alert(&quot;&quot; + &quot;修改成功&quot; + &quot;&quot;);<BR>
						<BR>
						parent.window.location.reload();	//刷新页面 <BR>
						parent.tb_remove();			//thickbox关闭窗口<BR>
					}else{<BR>
						alert( &quot;报警：&quot; + msg + &quot;&quot; );<BR>
					    ///document.write(msg)<BR>
					$(&quot;#modifybtn&quot;).toggle();	//提交成功时，再显示提交按钮；<BR>
						//parent.tb_remove();			//thickbox关闭窗口<BR>
					}},error: function(){alert( &quot;&quot; + &quot;页面错误&quot; + &quot;&quot; );}});<BR>
			});<BR>
		 });<BR>
	&lt;/script&gt;<BR>
    <BR>
    &lt;%<BR>
	Else<BR>
	%&gt;<BR>
    	&lt;font color=&quot;#FF0000&quot;&gt;No Records.&lt;/font&gt;<BR>
        <BR>
	&lt;%<BR>
	End If<BR>
	RS.Close<BR>
	Set RS = Nothing<BR>
	%&gt;<BR>
	<BR>
&lt;/body&gt;<BR>

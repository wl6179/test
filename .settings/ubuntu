<DT>zip</DT>
<!--property:icon_internal=blank-->
<!--property:syntax_highlight=sh-->
<!--property:date_created=1357483826-->
<!--property:date_modified=1441519668-->
<DD>
解压缩<BR>
unzip xxx.zip<BR>
<BR>
压缩<BR>
zip <b>-r</b>  abc.zip  abc/<BR>

</DD>
<DT>tar</DT>
<!--property:icon_internal=folder-->
<!--property:syntax_highlight=sh-->
<!--property:date_created=1357483826-->
<!--property:date_modified=1399728408-->
<!--property:expanded-->
<DD>
<DL>
<DT>gz</DT>
<!--property:icon_internal=blank-->
<!--property:syntax_highlight=sh-->
<!--property:date_created=1357483826-->
<!--property:date_modified=1441519824-->
<DD>
解压缩<BR>
tar zxvf xxx.tar.gz<BR>
<BR>
压缩<BR>
tar zcvf  abc.tar.gz  abc/<BR>

</DD>
<DT>bz2</DT>
<!--property:icon_internal=blank-->
<!--property:syntax_highlight=sh-->
<!--property:date_created=1357483826-->
<!--property:date_modified=1441519875-->
<DD>
解压缩<BR>
tar jxvf xxx.tar.bz2<BR>
<BR>
压缩<BR>
tar jcvf  abc.tar.bz2  abc/<BR>

</DD>
